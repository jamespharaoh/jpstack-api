<model-meta
	name="hybyteRouteOut"
	type="major">

	<fields>

		<!-- id -->

		<foreign-id-field
			field="route"/>

		<!-- identity -->

		<master-field
			type="route"/>

		<!-- settings -->

		<string-field
			name="url"/>

		<string-field
			name="username"/>

		<string-field
			name="password"/>

		<integer-field
			name="optInfo"/>

		<reference-field
			name="freeRoute"
			type="hybyteRouteOut"
			nullable="yes"/>

	</fields>

	<senders>

		<sender
			name="Hybyte"
			description="Hybyte sender"/>

	</senders>

</model-meta>