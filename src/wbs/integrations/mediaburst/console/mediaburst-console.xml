<console-module
	name="mediaburst">

	<simple-context
		name="mediaburst">

		<priv-key
			name="mediaburst.manage"
			delegate="root"
			priv="manage"/>

		<section
			name="proteusRoute"
			label="Proteus routes">

			<context-link
				name="out"
				link="mediaburstProteusRouteOut"
				label="Outbound"
				priv-key="mediaburst.manage"/>

		</section>

		<context-link
			name="network"
			link="mediaburstNetwork"
			label="Networks"
			priv-key="mediaburst.manage"/>

	</simple-context>

</console-module>
