<console-module
	name="mediaburst-network">

	<console-helper-provider
		object-name="mediaburstNetwork"
		view-priv="root:manage">

		<priv-key
			name="mediaburst.manage"
			priv="root:manage"/>

		<priv-key
			name="mediaburstNetwork.manage"
			priv="root:manage"/>

		<context-stuff
			name="mediaburstNetworkName"
			field="route.name"/>

	</console-helper-provider>

	<object-context
		name="mediaburstNetwork"
		object-name="mediaburstNetwork">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="mediaburst-network"
			type="read-only">

			<columns>

				<parent-field/>

				<integer-field
					name="otherId"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="mediaburst-network"
			type="create">

			<columns>

				<parent-field/>

				<integer-field
					name="otherId"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="mediaburst-network"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<integer-field
					name="otherId"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="mediaburst-network"
			type="update">

			<columns>

				<parent-field/>

				<integer-field
					name="otherId"/>

			</columns>

		</form>

	</forms>

</console-module>
