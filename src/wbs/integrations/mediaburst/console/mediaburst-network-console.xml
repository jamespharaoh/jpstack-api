<console-module
	name="mediaburst-network">

	<console-helper-provider
		object-name="mediaburstNetwork"
		view-priv="root:manage">

		<priv-key
			name="mediaburst.manage"
			priv="root:manage"/>

		<priv-key
			name="mediaburstNetwork.manage"
			priv="root:manage"/>

		<context-stuff
			name="mediaburstNetworkName"
			field="route.name"/>

	</console-helper-provider>

	<object-context
		name="mediaburstNetwork"
		object-name="mediaburstNetwork">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="mediaburstNetwork">

		<parent-field/>

		<integer-field
			name="otherId"/>

	</field-set>

	<field-set
		name="create"
		object-name="mediaburstNetwork">

		<parent-field/>

		<integer-field
			name="otherId"/>

	</field-set>

	<field-set
		name="summary"
		object-name="mediaburstNetwork">

		<id-field/>
		<parent-field/>

		<integer-field
			name="otherId"/>

	</field-set>

	<field-set
		name="settings"
		object-name="mediaburstNetwork">

		<parent-field/>

		<integer-field
			name="otherId"/>

	</field-set>

</console-module>
