<model-meta
	name="mediaburstProteusRouteOut"
	type="major">

	<fields>

		<!-- id -->

		<foreign-id-field
			field="route"/>

		<!-- identity -->

		<master-field
			type="route"/>

		<!-- settings -->

		<string-field
			name="url"/>

		<string-field
			name="username"/>

		<string-field
			name="password"/>

		<string-field
			name="servType"/>

	</fields>

	<senders>

		<sender
			name="Mediaburst Proteus"
			description="Mediaburst Proteus sender"/>

	</senders>

</model-meta>