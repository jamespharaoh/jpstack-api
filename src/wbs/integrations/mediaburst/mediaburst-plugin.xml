<plugin
	name="mediaburst"
	package="wbs.integrations.mediaburst">

	<plugin-dependencies>
		<plugin-dependency name="integrations-common"/>
		<plugin-dependency name="scaffold"/>
	</plugin-dependencies>

	<models>
		<model name="mediaburstProteusRouteOut"/>
		<model name="mediaburstNetwork"/>
	</models>

	<fixtures>
		<fixture name="mediaburst"/>
	</fixtures>

	<layer name="api">
		<bean class="api.MediaburstApiServletModule"/>
		<bean class="api.MediaburstProteusApiServletModule"/>
	</layer>

	<layer name="daemon">
		<bean class="daemon.MediaburstProteusSender"/>
	</layer>

	<console-modules>
		<console-module name="mediaburst"/>
		<console-module name="mediaburst-network"/>
		<console-module name="mediaburst-proteus-route-out"/>
	</console-modules>

</plugin>
