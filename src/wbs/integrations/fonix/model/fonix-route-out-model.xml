<model-meta
	name="fonixRouteOut"
	type="major">

	<fields>

		<!-- id -->

		<foreign-id-field
			field="route"/>

		<!-- identity -->

		<master-field
			type="route"/>

		<!-- details -->

		<deleted-field/>

		<!-- settings -->

		<reference-field
			type="fonixConfig"/>

		<string-field
			name="url"/>

		<string-field
			name="apiKey"/>

	</fields>

	<senders>

		<sender
			name="fonix"
			description="Fonix sender"/>

	</senders>

</model-meta>