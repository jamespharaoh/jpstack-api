<console-module
	name="fonix-route-out">

	<console-helper-provider
		object-name="fonixRouteOut"
		view-priv="root:manage">

		<priv-key
			name="fonix.manage"
			priv="root:manage"/>

		<priv-key
			name="fonixRouteOut.manage"
			priv="root:manage"/>

		<context-stuff
			name="fonixRouteOutName"
			field="route.name"/>

	</console-helper-provider>

	<object-context
		name="fonixRouteOut"
		object-name="fonixRouteOut">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="fonix-route-out"
			type="read-only">

			<columns>

				<parent-field/>

				<object-field
					name="fonixConfig"
					label="Config"
					object-type="fonixConfig"/>

				<text-field
					name="url"/>

				<text-field
					name="apiKey"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="fonix-route-out"
			type="create">

			<columns>

				<parent-field/>

				<object-field
					name="fonixConfig"
					label="Config"
					object-type="fonixConfig"/>

				<text-field
					name="url"/>

				<text-field
					name="apiKey"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="fonix-route-out"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="fonixConfig"
					label="Config"
					object-type="fonixConfig"/>

				<text-field
					name="url"/>

				<text-field
					name="apiKey"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="fonix-route-out"
			type="update">

			<columns>

				<object-field
					name="fonixConfig"
					label="Config"
					object-type="fonixConfig"/>

				<text-field
					name="url"/>

				<text-field
					name="apiKey"/>

			</columns>

		</form>

	</forms>

</console-module>