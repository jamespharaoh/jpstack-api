<console-module
	name="fonix-inbound-log">

	<console-helper-provider
		object-name="fonixInboundLog"
		view-priv="root:manage">

		<priv-key
			name="fonixInboundLog.manage"
			priv="root:manage"/>

		<context-stuff
			name="fonixInboundLogName"
			field="id"/>

	</console-helper-provider>

	<object-context
		name="fonixInboundLog"
		object-name="fonixInboundLog">

		<list>

			<object-search-page
				search-fields="search"
				results-column-fields="results"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="search"
		class="wbs.integrations.fonix.model.FonixInboundLogSearch">

		<object-field
			name="routeId"
			label="Route"
			object-type="route"
			nullable="yes"/>

		<interval-field
			name="timestamp"
			nullable="yes"/>

		<text-field
			name="details"
			nullable="yes"/>

		<enum-field
			name="type"
			nullable="yes"/>

		<yes-no-field
			name="success"
			nullable="yes"/>

	</field-set>

	<field-set
		name="results"
		object-name="fonixInboundLog">

		<timestamp-field
			name="timestamp"/>

		<object-field
			name="route"
			object-type="route"/>

		<enum-field
			name="type"/>

		<yes-no-field
			name="success"/>

	</field-set>

	<field-set
		name="summary"
		object-name="fonixInboundLog">

		<id-field/>

		<timestamp-field
			name="timestamp"/>

		<object-field
			name="route"
			object-type="route"/>

		<enum-field
			name="type"/>

		<yes-no-field
			name="success"/>

		<text-area-field
			name="details"/>

	</field-set>

</console-module>
