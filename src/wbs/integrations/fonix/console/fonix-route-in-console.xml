<console-module
	name="fonix-route-in">

	<console-helper-provider
		object-name="fonixRouteIn"
		view-priv="root:manage">

		<priv-key
			name="fonix.manage"
			priv="root:manage"/>

		<priv-key
			name="fonixRouteIn.manage"
			priv="root:manage"/>

		<context-stuff
			name="fonixRouteInName"
			field="route.name"/>

	</console-helper-provider>

	<object-context
		name="fonixRouteIn"
		object-name="fonixRouteIn">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="fonix-route-in"
			type="read-only">

			<columns>

				<parent-field/>

				<object-field
					name="fonixConfig"
					label="Config"
					object-type="fonixConfig"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="fonix-route-in"
			type="create">

			<columns>

				<parent-field/>

				<object-field
					name="fonixConfig"
					label="Config"
					object-type="fonixConfig"/>

			</columns>

		</form>i

		<form
			name="summary"
			object-type="fonix-route-in"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="fonixConfig"
					label="Config"
					object-type="fonixConfig"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="fonix-route-in"
			type="update">

			<columns>

				<object-field
					name="fonixConfig"
					label="Config"
					object-type="fonixConfig"/>

			</columns>

		</form>

	</forms>

</console-module>
