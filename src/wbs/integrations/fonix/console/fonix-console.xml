<console-module
	name="fonix">

	<simple-context
		name="fonix">

		<priv-key
			name="fonix.manage"
			delegate="root"
			priv="manage"/>

		<context-link
			name="config"
			link="fonixConfig"
			label="Configs"
			priv-key="fonix.manage"/>

		<section
			name="route"
			label="Routes">

			<context-link
				name="out"
				link="fonixRouteOut"
				label="Outbound routes"
				priv-key="fonix.manage"/>

			<context-link
				name="in"
				link="fonixRouteIn"
				label="Inbound routes"
				priv-key="fonix.manage"/>

		</section>

		<section
			name="log"
			label="Logs">

			<context-link
				name="Inbound"
				link="fonixInboundLog"
				label="Inbound"
				priv-key="fonix.manage"/>

		</section>

	</simple-context>

</console-module>
