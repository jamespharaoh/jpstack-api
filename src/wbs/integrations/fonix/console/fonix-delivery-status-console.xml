<console-module
	name="fonix-delivery-status">

	<console-helper-provider
		object-name="fonixDeliveryStatus"
		view-priv="root:manage">

		<priv-key
			name="fonixDeliveryStatus.manage"
			delegate="root"
			priv="manage"/>

		<context-stuff
			name="fonixDeliveryStatusName"
			field="code"/>

		<context-stuff
			name="fonixConfigId"
			delegate="fonixConfig"
			field="id"/>

		<run-post-processor
			name="fonixConfig"/>

	</console-helper-provider>

	<object-context
		name="fonixDeliveryStatus"
		object-name="fonixDeliveryStatus">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"
				create-priv-delegate="root"
				create-priv-code="manage"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="fonixDeliveryStatus">

		<code-field/>

		<description-field/>

		<enum-field
			name="messageStatus"/>

		<yes-no-field
			name="permanent"/>

	</field-set>

	<field-set
		name="create"
		object-name="fonixDeliveryStatus">

		<code-field/>

		<description-field/>

		<enum-field
			name="messageStatus"/>

		<yes-no-field
			name="permanent"/>

	</field-set>

	<field-set
		name="summary"
		object-name="fonixDeliveryStatus">

		<id-field/>
		<parent-field/>
		<code-field/>
		<description-field/>
		<deleted-field/>

		<enum-field
			name="messageStatus"/>

		<yes-no-field
			name="permanent"/>

	</field-set>

	<field-set
		name="settings"
		object-name="fonixDeliveryStatus">

		<code-field/>
		<description-field/>
		<deleted-field/>

		<enum-field
			name="messageStatus"/>

		<yes-no-field
			name="permanent"/>

	</field-set>

</console-module>
