<console-module
	name="fonix-delivery-status">

	<console-helper-provider
		object-name="fonixDeliveryStatus"
		view-priv="root:manage"
		create-priv="root:manage">

		<priv-key
			name="fonixDeliveryStatus.manage"
			priv="root:manage"/>

		<context-stuff
			name="fonixDeliveryStatusName"
			field="code"/>

		<context-stuff
			name="fonixConfigId"
			field="fonixConfig.id"/>

		<run-post-processor
			name="fonixConfig"/>

	</console-helper-provider>

	<object-context
		name="fonixDeliveryStatus"
		object-name="fonixDeliveryStatus">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="fonix-delivery-status"
			type="read-only">

			<columns>

				<code-field/>

				<description-field/>

				<enum-field
					name="messageStatus"/>

				<yes-no-field
					name="permanent"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="fonix-delivery-status"
			type="create">

			<columns>

				<code-field/>

				<description-field/>

				<enum-field
					name="messageStatus"/>

				<yes-no-field
					name="permanent"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="fonix-delivery-status"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>
				<description-field/>
				<deleted-field/>

				<enum-field
					name="messageStatus"/>

				<yes-no-field
					name="permanent"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="fonix-delivery-status"
			type="update">

			<columns>

				<code-field/>
				<description-field/>
				<deleted-field/>

				<enum-field
					name="messageStatus"/>

				<yes-no-field
					name="permanent"/>

			</columns>

		</form>

	</forms>

</console-module>
