<plugin
	name="digital-select"
	package="wbs.integrations.digitalselect">

	<models>
		<model name="digitalSelectRouteOut"/>
	</models>

	<layer name="console">
		<component class="console.DigitalSelectRouteSummaryAdditionalPart"/>
		<component class="console.DigitalSelectRouteSummaryAdditionalPartFactory"/>
	</layer>

	<layer name="api">
		<component class="api.DigitalSelectApiServletModule"/>
		<component class="api.DigitalSelectRoutePathHandlerEntry"/>
		<component class="api.DigitalSelectRouteReportFile"/>
	</layer>

	<layer name="daemon">
		<component class="daemon.DigitalSelectSender"/>
	</layer>

</plugin>
