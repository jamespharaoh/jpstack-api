<console-module
	name="oxygenate-network">

	<console-helper-provider
		object-name="oxygenateNetwork"
		view-priv="root:manage">

		<priv-key
			name="oxygenateNetwork.manage"
			priv="root:manage"/>

		<context-stuff
			name="oxygenateNetworkName"
			field="network.name"/>

		<context-stuff
			name="oxygenateConfigId"
			field="oxygenateConfig.id"/>

		<run-post-processor
			name="oxygenateConfig"/>

	</console-helper-provider>

	<object-context
		name="oxygenateNetwork"
		object-name="oxygenateNetwork">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"
				create-priv-delegate="root"
				create-priv-code="manage"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="oxygenate-network"
			type="read-only">

			<columns>

				<object-field
					name="network"
					object-type="network"/>

				<text-field
					name="channel"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="oxygenate-network"
			type="create">

			<columns>

				<object-field
					name="network"
					object-type="network"/>

				<text-field
					name="channel"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="oxygenate-network"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="network"
					object-type="network"/>

				<text-field
					name="channel"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="oxygenate-network"
			type="update">

			<columns>

				<object-field
					name="network"
					object-type="network"/>

				<text-field
					name="channel"/>

			</columns>

		</form>

	</forms>

</console-module>
