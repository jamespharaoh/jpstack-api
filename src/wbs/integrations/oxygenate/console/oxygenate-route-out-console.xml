<console-module
	name="oxygenate-route-out">

	<console-helper-provider
		object-name="oxygenateRouteOut"
		view-priv="root:manage">

		<priv-key
			name="oxygenate.manage"
			priv="root:manage"/>

		<priv-key
			name="oxygenateRouteOut.manage"
			priv="root:manage"/>

		<context-stuff
			name="oxygenateRouteOutName"
			field="route.name"/>

	</console-helper-provider>

	<object-context
		name="oxygenateRouteOut"
		object-name="oxygenateRouteOut">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="oxygenate-route-out"
			type="read-only">

			<columns>

				<parent-field/>

				<object-field
					name="oxygenateConfig"
					label="Config"
					object-type="oxygenateConfig"/>

				<text-field
					name="shortcode"/>

				<yes-no-field
					name="premium"/>

				<text-field
					name="username"/>

				<text-field
					name="campaignId"/>

				<yes-no-field
					name="multipart"
					label="Multi"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="oxygenate-route-out"
			type="create">

			<columns>

				<parent-field/>

				<object-field
					name="oxygenateConfig"
					object-type="oxygenateConfig"/>

				<text-field
					name="relayUrl"/>

				<text-field
					name="shortcode"/>

				<yes-no-field
					name="premium"/>

				<text-field
					name="username"/>

				<text-field
					name="password"/>

				<text-field
					name="campaignId"
					nullable="yes"/>

				<yes-no-field
					name="multipart"/>

				<seconds-field
					name="connectTimeout"/>

				<seconds-field
					name="readTimeout"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="oxygenate-route-out"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="oxygenateConfig"
					object-type="oxygenateConfig"/>

				<text-field
					name="relayUrl"/>

				<text-field
					name="shortcode"/>

				<yes-no-field
					name="premium"/>

				<text-field
					name="username"/>

				<text-field
					name="password"/>

				<text-field
					name="campaignId"/>

				<yes-no-field
					name="multipart"/>

				<seconds-field
					name="connectTimeout"/>

				<seconds-field
					name="readTimeout"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="oxygenate-route-out"
			type="update">

			<columns>

				<parent-field/>

				<object-field
					name="oxygenateConfig"
					object-type="oxygenateConfig"/>

				<text-field
					name="relayUrl"/>

				<text-field
					name="shortcode"/>

				<yes-no-field
					name="premium"/>

				<text-field
					name="username"/>

				<text-field
					name="password"/>

				<text-field
					name="campaignId"
					nullable="yes"/>

				<yes-no-field
					name="multipart"/>

				<seconds-field
					name="connectTimeout"/>

				<seconds-field
					name="readTimeout"/>

			</columns>

		</form>

	</forms>

</console-module>
