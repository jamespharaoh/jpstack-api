<console-module
	name="oxygenate-report-code">

	<console-helper-provider
		object-name="oxygenateReportCode"
		view-priv="root:manage">

		<priv-key
			name="oxygenateReportCode.manage"
			priv="root:manage"/>

		<context-stuff
			name="oxygenateReportCodeName"
			field="code"/>

		<context-stuff
			name="oxygenateConfigId"
			field="oxygenateConfig.id"/>

		<run-post-processor
			name="oxygenateConfig"/>

	</console-helper-provider>

	<object-context
		name="oxygenateReportCode"
		object-name="oxygenateReportCode">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"
				create-priv-delegate="root"
				create-priv-code="manage"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="oxygenate-report-code"
			type="read-only">

			<columns>

				<code-field/>
				<description-field/>

				<enum-field
					name="messageStatus"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="oxygenate-report-code"
			type="create">

			<columns>

				<code-field pattern=".+"/>
				<description-field/>

				<enum-field
					name="messageStatus"/>

				<text-area-field
					name="additionalInformation"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="oxygenate-report-code"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>
				<description-field/>

				<enum-field
					name="messageStatus"/>

				<text-area-field
					name="additionalInformation"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="oxygenate-report-code"
			type="update">

			<columns>

				<code-field pattern=".+"/>
				<description-field/>

				<enum-field
					name="messageStatus"/>

				<text-area-field
					name="additionalInformation"/>

			</columns>

		</form>

	</forms>

</console-module>
