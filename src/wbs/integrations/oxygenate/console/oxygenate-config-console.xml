<console-module
	name="oxygenate-config">

	<console-helper-provider
		object-name="oxygenateConfig"
		view-priv="root:manage">

		<priv-key
			name="oxygenate.manage"
			priv="root:manage"/>

		<priv-key
			name="oxygenateConfig.manage"
			priv="root:manage"/>

		<context-stuff
			name="oxygenateConfigName"
			field="name"/>

	</console-helper-provider>

	<object-context
		name="oxygenateConfig"
		object-name="oxygenateConfig">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<context-link
				name="network"
				link="oxygenateNetwork"
				label="Networks"
				priv-key="oxygen8.manage"/>

			<context-link
				name="reportCode"
				link="oxygenateReportCode"
				label="Report codes"
				priv-key="oxygenate.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="oxygenate-config"
			type="read-only">

			<columns>

				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="create"
			object-type="oxygenate-config"
			type="create">

			<columns>

				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="oxygenate-config"
			type="read-only">

			<columns>

				<id-field/>
				<code-field/>
				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="oxygenate-config"
			type="update">

			<columns>

				<name-field/>
				<description-field/>

			</columns>

		</form>

	</forms>

</console-module>
