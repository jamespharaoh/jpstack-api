<plugin
	name="oxygenate"
	package="wbs.integrations.oxygenate">

	<plugin-dependencies>
		<plugin-dependency name="integrations-common"/>
		<plugin-dependency name="platform-scaffold"/>
	</plugin-dependencies>

	<models>
		<enum-type name="oxygenateRouteInType"/>
		<enum-type name="oxygenateInboundLogType"/>
		<model name="oxygenateConfig"/>
		<model name="oxygenateInboundLog"/>
		<model name="oxygenateNetwork"/>
		<model name="oxygenateReportCode"/>
		<model name="oxygenateRouteIn"/>
		<model name="oxygenateRouteOut"/>
	</models>

	<fixtures>
		<fixture name="oxygenate"/>
	</fixtures>

	<layer name="api">
		<component class="api.OxygenateRouteInApiAction"/>
		<component class="api.OxygenateRouteInMmsNewApiLoggingAction"/>
		<component class="api.OxygenateRouteInMmsNewRequestBuilder"/>
		<component class="api.OxygenateRouteInMmsOldApiAction"/>
		<component class="api.OxygenateRouteInSmsApiAction"/>
		<component class="api.OxygenateRouteReportApiLoggingAction"/>
	</layer>

	<layer name="console">
		<component class="console.OxygenateRouteSummaryAdditionalPart"/>
		<component class="console.OxygenateRouteSummaryAdditionalPartFactory"/>
	</layer>

	<layer name="daemon">
		<!--<component class="daemon.OxygenateInboundLogRemovalDaemon"/>-->
		<component class="daemon.OxygenateSmsSenderService"/>
		<component class="daemon.OxygenateSmsSenderServiceHelper"/>
	</layer>

	<layer name="logic">
		<component class="foreignapi.OxygenateSmsSender"/>
		<component class="foreignapi.OxygenateSmsSendHelper"/>
	</layer>

	<api-modules>
		<api-module name="oxygenate"/>
	</api-modules>

	<console-modules>
		<console-module name="oxygenate"/>
		<console-module name="oxygenate-config"/>
		<console-module name="oxygenate-inbound-log"/>
		<console-module name="oxygenate-network"/>
		<console-module name="oxygenate-report-code"/>
		<console-module name="oxygenate-route-in"/>
		<console-module name="oxygenate-route-out"/>
	</console-modules>

</plugin>
