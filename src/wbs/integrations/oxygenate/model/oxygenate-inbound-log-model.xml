<model-meta
	name="oxygenateInboundLog"
	old-name="oxygen8InboundLog"
	table="oxygen8_inbound_log"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field
			sequence="oxygen8_inbound_log_id_seq"/>

		<!-- identity -->

		<parent-field
			type="route"/>

		<timestamp-field
			name="timestamp"
			column-type="iso"/>

		<!-- data -->

		<enum-field
			name="type"
			type="oxygenateInboundLogType"/>

		<string-field
			name="details"/>

		<yes-no-field
			name="success"/>

	</fields>

	<background-processes>

		<background-process
			object-type="oxygen8-inbound-log"
			name="Removal"
			description="Remove old oxygenate inbound logs"
			frequency="60 seconds"/>

	</background-processes>

</model-meta>