<console-module
	name="txt-nation-network">

	<console-helper-provider
		object-name="txtNationNetwork"
		view-priv="root:manage">

		<context-stuff
			name="txtNationNetworkName"
			field="code"/>

		<priv-key
			name="txtNationNetwork.manage"
			priv="root:manage"/>

	</console-helper-provider>

	<object-context
		name="txtNationNetwork"
		object-name="txtNationNetwork">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="txt-nation-network"
			type="read-only">

			<columns>

				<code-field/>

				<object-field
					name="network"
					object-type="network"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="txt-nation-network"
			type="create">

			<columns>

				<code-field/>

				<object-field
					name="network"
					object-type="network"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="txt-nation-network"
			type="read-only">

			<columns>

				<id-field/>
				<code-field/>
				<deleted-field/>

				<object-field
					name="network"
					object-type="network"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="txt-nation-network"
			type="update">

			<columns>

				<id-field/>
				<code-field/>
				<deleted-field/>

				<object-field
					name="network"
					object-type="network"/>

			</columns>

		</form>

	</forms>

</console-module>
