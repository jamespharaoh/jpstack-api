<console-module
	name="txt-nation-route-in">

	<console-helper-provider
		object-name="txtNationRouteIn"
		view-priv="root:manage">

		<context-stuff
			name="txtNationRouteInName"
			field="route.name"/>

		<priv-key
			name="txtNationRouteIn.manage"
			priv="root:manage"/>

	</console-helper-provider>

	<object-context
		name="txtNationRouteIn"
		object-name="txtNationRouteIn">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="txt-nation-route-in"
			type="read-only">

			<columns>

				<parent-field/>

			</columns>

		</form>

		<form
			name="create"
			object-type="txt-nation-route-in"
			type="create">

			<columns>

				<parent-field/>

				<object-field
					name="numberFormat"
					object-type="numberFormat"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="txt-nation-route-in"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<deleted-field/>

				<object-field
					name="numberFormat"
					object-type="numberFormat"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="txt-nation-route-in"
			type="update">

			<columns>

				<id-field/>
				<parent-field read-only="yes"/>
				<deleted-field/>

				<object-field
					name="numberFormat"
					object-type="numberFormat"/>

			</columns>

		</form>

	</forms>

</console-module>
