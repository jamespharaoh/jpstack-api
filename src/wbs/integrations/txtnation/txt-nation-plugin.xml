<plugin
	name="txt-nation"
	package="txtnation">

	<sql-scripts>
		<sql-schema name="txt-nation"/>
		<sql-data name="txt-nation-data"/>
	</sql-scripts>

	<models>
		<model name="txtNationNetwork"/>
		<model name="txtNationRouteIn"/>
	</models>

	<layer name="api">
		<bean class="api.TxtNationApiServletModule"/>
		<bean class="api.TxtNationRouteInFile"/>
		<bean class="api.TxtNationRoutePathHandlerEntry"/>
	</layer>

	<console-modules>
		<console-module name="txt-nation"/>
		<console-module name="txt-nation-network"/>
		<console-module name="txt-nation-route-in"/>
	</console-modules>

</plugin>
