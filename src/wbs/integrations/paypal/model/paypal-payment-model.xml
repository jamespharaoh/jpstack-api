<model-meta
	name="paypalPayment"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="paypalAccount"/>

		<timestamp-field
			name="timestamp"
			column-type="sql"/>

		<!-- data -->

		<string-field
			name="paypalToken"
			nullable="yes"/>

		<integer-field
			name="value"/>

		<string-field
			name="paypalPayerId"
			nullable="yes"/>

		<!-- state -->

		<enum-field
			name="state"
			type="paypalPaymentState"/>

	</fields>

</model-meta>