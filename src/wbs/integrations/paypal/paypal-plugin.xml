<plugin
	name="paypal"
	package="wbs.paypal">

	<plugin-dependencies>
		<plugin-dependency name="sms-apps-common"/>
		<plugin-dependency name="scaffold"/>
	</plugin-dependencies>

	<sql-scripts>
		<sql-data name="paypal-data"/>
	</sql-scripts>

	<models>
		<model name="paypalAccount"/>
		<model name="paypalPayment"/>
	</models>

	<fixtures>
		<fixture name="paypal"/>
	</fixtures>

	<layer name="api">
		<bean class="api.PaypalApiLogicImplementation"/>
	</layer>

	<console-modules>
		<console-module name="paypal-account"/>
	</console-modules>

</plugin>
