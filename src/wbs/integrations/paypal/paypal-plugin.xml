<plugin
	name="paypal"
	package="wbs.integrations.paypal">

	<plugin-dependencies>
		<plugin-dependency name="integrations-common"/>
		<plugin-dependency name="platform-scaffold"/>
	</plugin-dependencies>

	<models>
		<enum-type name="paypalPaymentState"/>
		<model name="paypalAccount"/>
		<model name="paypalPayment"/>
	</models>

	<fixtures>
		<fixture name="paypal"/>
	</fixtures>

	<layer name="logic">
		<component class="logic.PaypalApiImplementation"/>
		<component class="logic.PaypalLogicImplementation"/>
	</layer>

	<console-modules>
		<console-module name="paypal-account"/>
		<console-module name="paypal-payment"/>
	</console-modules>

</plugin>
