<console-module
	name="paypal-account">

	<console-helper-provider
		object-name="paypalAccount">

		<context-stuff
			name="paypalAccountName"
			field="name"/>

		<context-stuff
			name="sliceId"
			field="slice.id"/>

		<priv-key
			name="paypalAccount.manage"
			priv="manage"/>

		<run-post-processor
			name="slice"/>

	</console-helper-provider>

	<object-context
		name="paypalAccount"
		object-name="paypalAccount">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="paypal-account"
			type="read-only">

			<columns>

				<parent-field/>
				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="create"
			object-type="paypal-account"
			type="create">

			<columns>

				<parent-field/>
				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="paypal-account"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>
				<name-field/>
				<description-field/>
				<deleted-field/>

				<text-field
					name="username"
					nullable="yes"/>

				<text-field
					name="password"
					nullable="yes"/>

				<text-field
					name="signature"
					nullable="yes"/>

				<text-field
					name="appId"
					nullable="yes"/>

				<text-field
					name="serviceEndpointPaypalApi"
					nullable="yes"/>

				<text-field
					name="serviceEndpointPaypalApiAa"
					nullable="yes"/>

				<text-field
					name="serviceEndpointPermissions"
					nullable="yes"/>

				<text-field
					name="serviceEndpointAdaptivePayments"
					nullable="yes"/>

				<text-field
					name="serviceEndpointAdaptiveAccounts"
					nullable="yes"/>

				<text-field
					name="serviceEndpointInvoice"
					nullable="yes"/>

				<text-field
					name="checkoutUrl"
					nullable="yes"/>

				<text-field
					name="mode"
					nullable="yes"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="paypal-account"
			type="update">

			<columns>

				<parent-field/>
				<name-field/>
				<description-field/>
				<deleted-field/>

				<text-field
					name="username"
					nullable="yes"/>

				<text-field
					name="password"
					nullable="yes"/>

				<text-field
					name="signature"
					nullable="yes"/>

				<text-field
					name="appId"
					nullable="yes"/>

				<text-field
					name="serviceEndpointPaypalApi"
					nullable="yes"/>

				<text-field
					name="serviceEndpointPaypalApiAa"
					nullable="yes"/>

				<text-field
					name="serviceEndpointPermissions"
					nullable="yes"/>

				<text-field
					name="serviceEndpointAdaptivePayments"
					nullable="yes"/>

				<text-field
					name="serviceEndpointAdaptiveAccounts"
					nullable="yes"/>

				<text-field
					name="serviceEndpointInvoice"
					nullable="yes"/>

				<text-field
					name="checkoutUrl"
					nullable="yes"/>

				<text-field
					name="mode"
					nullable="yes"/>

			</columns>

		</form>

	</forms>

</console-module>
