<console-module
	name="paypal-account">

	<console-helper-provider
		object-name="paypalAccount">

		<context-stuff
			name="paypalAccountName"
			field="name"/>

		<context-stuff
			name="sliceId"
			delegate="slice"
			field="id"/>

		<priv-key
			name="paypalAccount.manage"
			priv="manage"/>

		<run-post-processor
			name="slice"/>

	</console-helper-provider>

	<object-context
		name="paypalAccount"
		object-name="paypalAccount">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="paypalAccount">

		<parent-field/>
		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="create"
		object-name="paypalAccount">

		<parent-field/>
		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="paypalAccount">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-field
			name="username"
			nullable="yes"/>

		<text-field
			name="password"
			nullable="yes"/>

		<text-field
			name="signature"
			nullable="yes"/>

		<text-field
			name="appId"
			nullable="yes"/>

		<text-field
			name="serviceEndpointPaypalApi"
			nullable="yes"/>

		<text-field
			name="serviceEndpointPaypalApiAa"
			nullable="yes"/>

		<text-field
			name="serviceEndpointPermissions"
			nullable="yes"/>

		<text-field
			name="serviceEndpointAdaptivePayments"
			nullable="yes"/>

		<text-field
			name="serviceEndpointAdaptiveAccounts"
			nullable="yes"/>

		<text-field
			name="serviceEndpointInvoice"
			nullable="yes"/>

		<text-field
			name="checkoutUrl"
			nullable="yes"/>

		<text-field
			name="mode"
			nullable="yes"/>

	</field-set>

	<field-set
		name="settings"
		object-name="paypalAccount">

		<parent-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-field
			name="username"
			nullable="yes"/>

		<text-field
			name="password"
			nullable="yes"/>

		<text-field
			name="signature"
			nullable="yes"/>

		<text-field
			name="appId"
			nullable="yes"/>

		<text-field
			name="serviceEndpointPaypalApi"
			nullable="yes"/>

		<text-field
			name="serviceEndpointPaypalApiAa"
			nullable="yes"/>

		<text-field
			name="serviceEndpointPermissions"
			nullable="yes"/>

		<text-field
			name="serviceEndpointAdaptivePayments"
			nullable="yes"/>

		<text-field
			name="serviceEndpointAdaptiveAccounts"
			nullable="yes"/>

		<text-field
			name="serviceEndpointInvoice"
			nullable="yes"/>

		<text-field
			name="checkoutUrl"
			nullable="yes"/>

		<text-field
			name="mode"
			nullable="yes"/>

	</field-set>

</console-module>
