<model-meta
	name="oxygen8RouteOut"
	type="major">

	<fields>

		<!-- id -->

		<foreign-id-field
			field="route"/>

		<!-- identity -->

		<master-field
			type="route"/>

		<!-- settings -->

		<reference-field
			type="oxygen8Config"/>

		<string-field
			name="relayUrl"/>

		<string-field
			name="shortcode"/>

		<yes-no-field
			name="premium"/>

		<string-field
			name="username"/>

		<string-field
			name="password"/>

		<string-field
			name="campaignId"
			nullable="yes"/>

		<yes-no-field
			name="multipart"
			default="no"/>

		<integer-field
			name="connectTimeout"
			default="60"/>

		<integer-field
			name="readTimeout"
			default="60"/>

	</fields>

	<senders>

		<sender
			name="oxygen8"
			description="Oxygen8 SMS sender"/>

	</senders>

</model-meta>