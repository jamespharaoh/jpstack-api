<console-module
	name="oxygen8-network">

	<console-helper-provider
		object-name="oxygen8Network"
		view-priv="root:manage">

		<priv-key
			name="oxygen8Network.manage"
			priv="root:manage"/>

		<context-stuff
			name="oxygen8NetworkName"
			field="network.name"/>

		<context-stuff
			name="oxygen8ConfigId"
			field="oxygen8Config.id"/>

		<run-post-processor
			name="oxygen8Config"/>

	</console-helper-provider>

	<object-context
		name="oxygen8Network"
		object-name="oxygen8Network">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"
				create-priv-delegate="root"
				create-priv-code="manage"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="oxygen8Network">

		<object-field
			name="network"
			object-type="network"/>

		<text-field
			name="channel"/>

	</field-set>

	<field-set
		name="create"
		object-name="oxygen8Network">

		<object-field
			name="network"
			object-type="network"/>

		<text-field
			name="channel"/>

	</field-set>

	<field-set
		name="summary"
		object-name="oxygen8Network">

		<id-field/>
		<parent-field/>

		<object-field
			name="network"
			object-type="network"/>

		<text-field
			name="channel"/>

	</field-set>

	<field-set
		name="settings"
		object-name="oxygen8Network">

		<object-field
			name="network"
			object-type="network"/>

		<text-field
			name="channel"/>

	</field-set>

</console-module>
