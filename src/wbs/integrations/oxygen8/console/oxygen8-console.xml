<console-module
	name="oxygen8">

	<simple-context
		name="oxygen8">

		<priv-key
			name="oxygen8.manage"
			priv="root:manage"/>

		<context-link
			name="config"
			link="oxygen8Config"
			label="Configs"
			priv-key="oxygen8.manage"/>

		<section
			name="route"
			label="Routes">

			<context-link
				name="in"
				link="oxygen8RouteIn"
				label="Inbound routes routes"
				priv-key="oxygen8.manage"/>

			<context-link
				name="out"
				link="oxygen8RouteOut"
				label="Outbound routes"
				priv-key="oxygen8.manage"/>

		</section>

		<section
			name="log"
			label="Logs">

			<context-link
				name="Inbound"
				link="oxygen8InboundLog"
				label="Inbound"
				priv-key="oxygen8.manage"/>

		</section>

	</simple-context>

</console-module>
