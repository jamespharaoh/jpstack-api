<plugin
	name="oxygen8"
	package="wbs.integrations.oxygen8">

	<plugin-dependencies>
		<plugin-dependency name="integrations-common"/>
		<plugin-dependency name="scaffold"/>
	</plugin-dependencies>

	<models>
		<enum-type name="oxygen8InboundLogType"/>
		<model name="oxygen8Config"/>
		<model name="oxygen8InboundLog"/>
		<model name="oxygen8Network"/>
		<model name="oxygen8ReportCode"/>
		<model name="oxygen8RouteIn"/>
		<model name="oxygen8RouteOut"/>
	</models>

	<fixtures>
		<fixture name="oxygen8"/>
	</fixtures>

	<layer name="api">
		<component class="api.Oxygen8ApiServletModule"/>
		<component class="api.Oxygen8InboundFile"/>
		<component class="api.Oxygen8InboundMmsAction"/>
		<component class="api.Oxygen8InboundSmsAction"/>
		<component class="api.Oxygen8ReportFile"/>
	</layer>

	<layer name="console">
		<component class="console.Oxygen8RouteSummaryAdditionalPart"/>
		<component class="console.Oxygen8RouteSummaryAdditionalPartFactory"/>
	</layer>

	<layer name="daemon">
		<component class="daemon.Oxygen8Sender"/>
	</layer>

	<console-modules>
		<console-module name="oxygen8"/>
		<console-module name="oxygen8-config"/>
		<console-module name="oxygen8-inbound-log"/>
		<console-module name="oxygen8-network"/>
		<console-module name="oxygen8-report-code"/>
		<console-module name="oxygen8-route-in"/>
		<console-module name="oxygen8-route-out"/>
	</console-modules>

</plugin>
