<console-module
	name="sms-arena-route-in">

	<console-helper-provider
		object-name="smsArenaRouteIn"
		view-delegate-field="root"
		view-delegate-priv-code="manage">

		<priv-key
			name="smsArena_manage"
			delegate="root"
			priv="manage"/>

		<priv-key
			name="smsArenaRouteIn_manage"
			delegate="root"
			priv="manage"/>

		<context-stuff
			name="smsArenaRouteInName"
			delegate="route"
			field="name"/>

	</console-helper-provider>

	<object-context
		name="smsArenaRouteIn"
		object-name="smsArenaRouteIn">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="smsArenaRouteIn">

		<parent-field/>

		<object-field
			name="smsArenaConfig"
			label="Config"
			finder="smsArenaConfig"/>

	</field-set>

	<field-set
		name="create"
		object-name="smsArenaRouteIn">

		<parent-field/>

		<object-field
			name="smsArenaConfig"
			finder="smsArenaConfig"/>

	</field-set>

	<field-set
		name="summary"
		object-name="smsArenaRouteIn">

		<id-field/>
		<parent-field/>

		<object-field
			name="smsArenaConfig"
			finder="smsArenaConfig"/>

	</field-set>

	<field-set
		name="settings"
		object-name="smsArenaRouteIn">

		<parent-field/>

		<object-field
			name="smsArenaConfig"
			finder="smsArenaConfig"/>

	</field-set>

</console-module>