<console-module
	name="sms-arena-config">

	<console-helper-provider
		object-name="smsArenaConfig"
		view-delegate-field="root"
		view-delegate-priv-code="manage">

		<priv-key
			name="smsArena.manage"
			delegate="root"
			priv="manage"/>

		<priv-key
			name="smsArenaConfig.manage"
			delegate="root"
			priv="manage"/>

		<context-stuff
			name="smsArenaConfigName"
			field="name"/>

	</console-helper-provider>

	<object-context
		name="smsArenaConfig"
		object-name="smsArenaConfig">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<context-link
				name="reportCode"
				link="smsArenaReportCode"
				label="Report codes"
				priv-key="smsArena.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="smsArenaConfig">

		<parent-field/>
		<name-field/>
		<description-field/>

	</field-set>

		<field-set
		name="create"
		object-name="smsArenaConfig">

		<parent-field/>
		<name-field/>
		<description-field/>

		<integer-field
			name="profileId"/>

	</field-set>

		<field-set
		name="summary"
		object-name="smsArenaConfig">

		<parent-field/>
		<name-field/>
		<description-field/>

		<integer-field
			name="profileId"/>

	</field-set>

		<field-set
		name="settings"
		object-name="smsArenaConfig">

		<parent-field/>
		<name-field/>
		<description-field/>

		<integer-field
			name="profileId"/>

	</field-set>

</console-module>