<model-meta
	name="smsArenaRouteOut"
	type="major">

	<fields>

		<!-- id -->

		<foreign-id-field
			field="route"/>

		<!-- identity -->

		<master-field
			type="route"/>

		<!-- settings -->

		<reference-field
			type="smsArenaConfig"/>

		<string-field
			name="relayUrl"/>

		<string-field
			name="authKey"/>

		<string-field
			name="cid"
			nullable="yes"/>

		<string-field
			name="pid"
			nullable="yes"/>

		<string-field
			name="sid"
			nullable="yes"/>

		<integer-field
			name="mclass"
			nullable="yes"/>

		<string-field
			name="coding"
			nullable="yes"/>

		<string-field
			name="date"
			nullable="yes"/>

		<string-field
			name="validity"
			nullable="yes"/>

	</fields>

	<senders>

		<sender
			name="SMS Arena"
			description="SMS Arena sender"/>

	</senders>

</model-meta>