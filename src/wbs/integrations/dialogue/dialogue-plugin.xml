<plugin
	name="dialogue"
	package="wbs.integrations.dialogue">

	<models>
		<model name="dialogueLocator"/>
		<model name="dialogueMmsRoute"/>
	</models>

	<layer name="api">
		<bean class="api.DialogueApiServletModule"/>
		<bean class="api.DialogueMmsApiServletModule"/>
		<bean class="api.DialogueResponder"/>
	</layer>

	<layer name="daemon">
		<bean class="daemon.DialogueMmsSender"/>
	</layer>

	<console-modules>
		<console-module name="dialogue"/>
		<console-module name="dialogue-locator"/>
	</console-modules>

</plugin>
