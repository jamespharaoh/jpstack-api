<plugin
	name="dialogue"
	package="wbs.integrations.dialogue">

	<models>
		<model name="dialogueLocator"/>
		<model name="dialogueMmsRoute"/>
	</models>

	<layer name="api">
		<component class="api.DialogueApiServletModule"/>
		<component class="api.DialogueMmsApiServletModule"/>
		<component class="api.DialogueResponderFactory"/>
	</layer>

	<layer name="daemon">
		<component class="daemon.DialogueMmsSender"/>
	</layer>

	<console-modules>
		<console-module name="dialogue"/>
		<console-module name="dialogue-locator"/>
		<console-module name="dialogue-mms-route"/>
	</console-modules>

</plugin>
