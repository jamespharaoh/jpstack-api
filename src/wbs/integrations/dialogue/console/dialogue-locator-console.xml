<console-module
	name="dialogue-locator">

	<console-helper-provider
		object-name="dialogueLocator"
		view-priv="root:manage">

		<priv-key
			name="dialogueLocator.manage"
			priv="root:manage"/>

		<context-stuff
			name="dialogueLocatorName"
			field="name"/>

	</console-helper-provider>

	<object-context
		name="dialogueLocator"
		object-name="dialogueLocator">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="dialogue-locator"
			type="read-only">

			<columns>

				<parent-field/>
				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="create"
			object-type="dialogue-locator"
			type="create">

			<columns>

				<parent-field/>
				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="dialogue-locator"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>
				<name-field/>
				<description-field/>
				<deleted-field/>

				<text-field
					name="url"
					label="URL"/>

				<text-field
					name="account"/>

				<text-field
					name="organiser"/>

				<text-field
					name="password"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="dialogue-locator"
			type="update">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>
				<name-field/>
				<description-field/>
				<deleted-field/>

				<text-field
					name="url"
					label="URL"/>

				<text-field
					name="account"/>

				<text-field
					name="organiser"/>

				<text-field
					name="password"/>

			</columns>

		</form>

	</forms>

</console-module>
