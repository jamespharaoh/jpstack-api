<model-meta
	name="clockworkSmsInboundLog"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="route"/>

		<timestamp-field
			name="timestamp"
			column-type="iso"/>

		<!-- data -->

		<enum-field
			name="type"
			type="clockworkSmsInboundLogType"/>

		<string-field
			name="details"/>

		<yes-no-field
			name="success"/>

	</fields>

	<background-processes>

		<background-process
			object-type="clockwork-sms-inbound-log"
			name="Removal"
			description="Remove old Clockwork SMS inbound logs"
			frequency="60 seconds"/>

	</background-processes>

</model-meta>