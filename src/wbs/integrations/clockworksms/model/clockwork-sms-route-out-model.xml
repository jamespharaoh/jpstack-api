<model-meta
	name="clockworkSmsRouteOut"
	type="major">

	<fields>

		<!-- id -->

		<foreign-id-field
			field="route"/>

		<!-- identity -->

		<master-field
			type="route"/>

		<!-- settings -->

		<reference-field
			type="clockworkSmsConfig"/>

		<string-field
			name="url"/>

		<string-field
			name="key"/>

		<integer-field
			name="maxParts"
			minimum="1"
			maximum="3"
			default="1"/>

	</fields>

	<senders>

		<sender
			name="clockwork_sms"
			description="Clockwork SMS sender"/>

	</senders>

</model-meta>