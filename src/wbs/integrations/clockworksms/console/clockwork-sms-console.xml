<console-module
	name="clockwork-sms">

	<simple-context
		name="clockworkSms">

		<priv-key
			name="clockworkSms.manage"
			priv="root:manage"/>

		<context-link
			name="config"
			link="clockworkSmsConfig"
			label="Configs"
			priv-key="clockworkSms.manage"/>

		<section
			name="route"
			label="Routes">

			<context-link
				name="out"
				link="clockworkSmsRouteOut"
				label="Outbound routes"
				priv-key="clockworkSms.manage"/>

			<context-link
				name="in"
				link="clockworkSmsRouteIn"
				label="Inbound routes"
				priv-key="clockworkSms.manage"/>

		</section>

		<section
			name="log"
			label="Logs">

			<context-link
				name="Inbound"
				link="clockworkSmsInboundLog"
				label="Inbound"
				priv-key="clockworkSms.manage"/>

		</section>

	</simple-context>

</console-module>
