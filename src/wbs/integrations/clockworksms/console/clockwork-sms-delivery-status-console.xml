<console-module
	name="clockwork-sms-delivery-status">

	<console-helper-provider
		object-name="clockworkSmsDeliveryStatus"
		view-priv="root:manage">

		<priv-key
			name="clockworkSmsDeliveryStatus.manage"
			delegate="root"
			priv="manage"/>

		<context-stuff
			name="clockworkSmsDeliveryStatusName"
			field="code"/>

		<context-stuff
			name="clockworkSmsConfigId"
			delegate="clockworkSmsConfig"
			field="id"/>

		<run-post-processor
			name="clockworkSmsConfig"/>

	</console-helper-provider>

	<object-context
		name="clockworkSmsDeliveryStatus"
		object-name="clockworkSmsDeliveryStatus">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"
				create-priv-delegate="root"
				create-priv-code="manage"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="clockworkSmsDeliveryStatus">

		<code-field/>

		<description-field/>

		<enum-field
			name="messageStatus"/>

		<text-field
			name="theirDescription"/>

	</field-set>

	<field-set
		name="create"
		object-name="clockworkSmsDeliveryStatus">

		<code-field/>

		<description-field/>

		<enum-field
			name="messageStatus"/>

		<text-field
			name="theirDescription"/>

		<text-area-field
			name="theirCommonCauses"
			nullable="yes"/>

	</field-set>

	<field-set
		name="summary"
		object-name="clockworkSmsDeliveryStatus">

		<id-field/>
		<parent-field/>
		<code-field/>
		<description-field/>
		<deleted-field/>

		<enum-field
			name="messageStatus"/>

		<text-field
			name="theirDescription"/>

		<text-area-field
			name="theirCommonCauses"/>

	</field-set>

	<field-set
		name="settings"
		object-name="clockworkSmsDeliveryStatus">

		<code-field/>
		<description-field/>
		<deleted-field/>

		<enum-field
			name="messageStatus"/>

		<text-field
			name="theirDescription"/>

		<text-area-field
			name="theirCommonCauses"
			nullable="yes"/>

	</field-set>

</console-module>
