<console-module
	name="clockwork-sms-config">

	<console-helper-provider
		object-name="clockworkSmsConfig"
		view-priv="root:manage">

		<priv-key
			name="clockworkSms.manage"
			delegate="root"
			priv="manage"/>

		<priv-key
			name="clockworkSmsConfig.manage"
			delegate="root"
			priv="manage"/>

		<context-stuff
			name="clockworkSmsConfigName"
			field="name"/>

	</console-helper-provider>

	<object-context
		name="clockworkSmsConfig"
		object-name="clockworkSmsConfig">

		<list>

			<object-list-page/>

			<object-create-page/>

		</list>

		<object>

			<object-summary-page/>

			<object-settings-page/>

			<object-events-page/>

		</object>

	</object-context>

</console-module>
