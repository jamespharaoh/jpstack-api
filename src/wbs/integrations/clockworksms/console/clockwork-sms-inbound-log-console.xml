<console-module
	name="clockwork-sms-inbound-log">

	<console-helper-provider
		object-name="clockworkSmsInboundLog"
		view-priv="root:manage">

		<priv-key
			name="clockworkSmsInboundLog.manage"
			priv="root:manage"/>

		<context-stuff
			name="clockworkSmsInboundLogName"
			field="id"/>

	</console-helper-provider>

	<object-context
		name="clockworkSmsInboundLog"
		object-name="clockworkSmsInboundLog">

		<list>

			<object-search-page
				search-class="wbs.integrations.clockworksms.model.ClockworkSmsInboundLogSearch"
				search-form="search"
				results-form="results"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

		</object>

	</object-context>

	<forms>

		<form
			name="search"
			class="wbs.integrations.clockworksms.model.ClockworkSmsInboundLogSearch"
			type="search">

			<columns>

				<object-field
					name="routeId"
					label="Route"
					object-type="route"
					nullable="yes"/>

				<interval-field
					name="timestamp"
					nullable="yes"/>

				<text-field
					name="details"
					nullable="yes"/>

				<enum-field
					name="type"
					nullable="yes"/>

				<yes-no-field
					name="success"
					nullable="yes"/>

			</columns>

		</form>

		<form
			name="results"
			object-type="clockwork-sms-inbound-log"
			type="read-only">

			<columns>

				<id-field/>

				<timestamp-field
					name="timestamp"/>

				<object-field
					name="route"
					object-type="route"/>

				<enum-field
					name="type"/>

				<yes-no-field
					name="success"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="clockwork-sms-inbound-log"
			type="read-only">

			<columns>

				<id-field/>

				<timestamp-field
					name="timestamp"/>

				<object-field
					name="route"
					object-type="route"/>

				<enum-field
					name="type"/>

				<yes-no-field
					name="success"/>

				<text-area-field
					name="details"/>

			</columns>

		</form>

	</forms>

</console-module>
