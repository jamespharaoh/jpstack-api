<console-module
	name="clockwork-sms-route-in">

	<console-helper-provider
		object-name="clockworkSmsRouteIn"
		view-priv="root:manage">

		<priv-key
			name="clockworkSms.manage"
			priv="root:manage"/>

		<priv-key
			name="clockworkSmsRouteIn.manage"
			priv="root:manage"/>

		<context-stuff
			name="clockworkSmsRouteInName"
			field="route.name"/>

	</console-helper-provider>

	<object-context
		name="clockworkSmsRouteIn"
		object-name="clockworkSmsRouteIn">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="clockwork-sms-route-in"
			type="read-only">

			<columns>

				<parent-field/>

				<object-field
					name="clockworkSmsConfig"
					label="Config"
					object-type="clockworkSmsConfig"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="clockwork-sms-route-in"
			type="create">

			<columns>

				<parent-field/>

				<object-field
					name="clockworkSmsConfig"
					label="Config"
					object-type="clockworkSmsConfig"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="clockwork-sms-route-in"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="clockworkSmsConfig"
					label="Config"
					object-type="clockworkSmsConfig"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="clockwork-sms-route-in"
			type="update">

			<columns>

				<object-field
					name="clockworkSmsConfig"
					label="Config"
					object-type="clockworkSmsConfig"/>

			</columns>

		</form>

	</forms>

</console-module>
