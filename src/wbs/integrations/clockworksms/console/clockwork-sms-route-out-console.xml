<console-module
	name="clockwork-sms-route-out">

	<console-helper-provider
		object-name="clockworkSmsRouteOut"
		view-priv="root:manage">

		<priv-key
			name="clockworkSms.manage"
			priv="root:manage"/>

		<priv-key
			name="clockworkSmsRouteOut.manage"
			priv="root:manage"/>

		<context-stuff
			name="clockworkSmsRouteOutName"
			field="route.name"/>

	</console-helper-provider>

	<object-context
		name="clockworkSmsRouteOut"
		object-name="clockworkSmsRouteOut">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="clockwork-sms-route-out"
			type="read-only">

			<columns>

				<parent-field/>

				<object-field
					name="clockworkSmsConfig"
					label="Config"
					object-type="clockworkSmsConfig"/>

				<text-field
					name="url"/>

				<text-field
					name="key"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="clockwork-sms-route-out"
			type="create">

			<columns>

				<parent-field/>

				<object-field
					name="clockworkSmsConfig"
					label="Config"
					object-type="clockworkSmsConfig"/>

				<text-field
					name="url"/>

				<text-field
					name="key"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="clockwork-sms-route-out"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="clockworkSmsConfig"
					label="Config"
					object-type="clockworkSmsConfig"/>

				<text-field
					name="url"/>

				<text-field
					name="key"/>

				<integer-field
					name="maxParts"/>

				<yes-no-field
					name="simulateMultipart"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="clockwork-sms-route-out"
			type="update">

			<columns>

				<object-field
					name="clockworkSmsConfig"
					label="Config"
					object-type="clockworkSmsConfig"/>

				<text-field
					name="url"/>

				<text-field
					name="key"/>

				<integer-field
					name="maxParts"
					minimum="1"
					maximum="3"/>

				<yes-no-field
					name="simulateMultipart"/>

			</columns>

		</form>

	</forms>

</console-module>