<model-meta
	name="imChatEvent"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="imChat"/>

		<timestamp-field
			name="timestamp"
			column-type="iso"/>

		<!-- data -->

		<timestamp-field
			name="clientTimestamp"
			column-type="iso"/>

		<timestamp-field
			name="adjustedTimestamp"
			column-type="iso"/>

		<string-field
			name="invocationToken"/>

		<integer-field
			name="index"/>

		<reference-field
			type="imChatSession"
			nullable="yes"/>

		<reference-field
			type="imChatCustomer"
			nullable="yes"/>

		<string-field
			name="source"/>

		<string-field
			name="type"/>

		<string-field
			name="payload"/>

	</fields>

</model-meta>