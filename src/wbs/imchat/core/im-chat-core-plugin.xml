<plugin
	name="im-chat-core"
	package="wbs.imchat.core">

	<plugin-dependencies>
		<plugin-dependency name="sms-apps-common"/>
	</plugin-dependencies>

	<sql-scripts>
		<sql-data name="im-chat-core-data"/>
	</sql-scripts>

	<models>
		<model name="imChat"/>
	</models>

	<fixtures>
		<fixture name="imChatCore"/>
	</fixtures>

	<layer name="api">
		<bean class="api.ImChatTestAction"/>
	</layer>

	<api-modules>
		<api-module name="im-chat"/>
	</api-modules>

	<console-modules>
		<console-module name="im-chat"/>
	</console-modules>

</plugin>
