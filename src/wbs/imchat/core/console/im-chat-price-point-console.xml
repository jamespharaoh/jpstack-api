<console-module
	name="im-chat-price-point">

	<console-helper-provider
		object-name="imChatPricePoint"
		view-delegate-field="imChat"
		view-delegate-priv-code="manage">

		<priv-key
			name="imChatPricePoint.manage"
			delegate="imChat"
			priv="manage"/>

		<context-stuff
			name="imChatPricePointName"
			field="code"/>

		<context-stuff
			name="imChatId"
			delegate="imChat"
			field="id"/>

		<run-post-processor
			name="imChat"/>

	</console-helper-provider>

	<object-context
		name="imChatPricePoint"
		object-name="imChatPricePoint">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>
			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="imChatPricePoint">

		<parent-field/>
		<code-field/>

	</field-set>

	<field-set
		name="results"
		object-name="imChatPricePoint">

		<code-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="imChatPricePoint">

		<id-field/>
		<parent-field/>
		<code-field/>

		<integer-field
			name="price"
			nullable="yes"/>

		<integer-field
			name="value"
			nullable="yes"/>

	</field-set>

</console-module>
