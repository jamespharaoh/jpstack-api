<console-module
	name="psychic-keyword">

	<console-helper-provider
		object-name="psychicKeyword">

		<context-stuff
			name="psychicKeywordName"
			field="keyword"/>

		<context-stuff
			name="psychicId"
			delegate="psychic"
			field="id"/>

		<run-post-processor
			name="psychic"/>

	</console-helper-provider>

	<object-context
		name="psychicKeyword"
		object-name="psychicKeyword">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="psychicKeyword">

		<code-field/>

		<enum-field
			name="type"
			helper="psychicKeywordTypeEnumHelper"/>

		<object-field
			name="targetCommand"
			label="Command"
			finder="command"/>

		<object-field
			name="joinAffiliate"
			label="Affiliate"
			finder="psychicAffiliate"
			root-field="psychic"/>

	</field-set>

	<field-set
		name="summary"
		object-name="psychicKeyword">

		<id-field/>
		<parent-field/>
		<code-field/>

		<enum-field
			name="type"
			helper="psychicKeywordTypeEnumHelper"/>

		<object-field
			name="targetCommand"
			label="Command"
				finder="command"/>

		<object-field
			name="joinAffiliate"
			label="Affiliate"
			finder="psychicAffiliate"
			root-field="psychic"/>

	</field-set>

	<field-set
		name="settings"
		object-name="psychicKeyword">

		<id-field/>
		<parent-field/>
		<code-field/>

		<enum-field
			name="type"
			helper="psychicKeywordTypeEnumHelper"/>

		<object-field
			name="targetCommand"
			label="Command"
			finder="command"/>

		<object-field
			name="joinAffiliate"
			label="Affiliate"
			finder="psychicAffiliate"
			root-field="psychic"/>

	</field-set>

</console-module>
