<plugin
	name="psychic-core"
	package="core">

	<sql-scripts>
		<sql-data name="psychic-data"/>
	</sql-scripts>

	<models>
		<model name="psychic"/>
		<model name="psychicRoutes"/>
		<model name="psychicSettings"/>
	</models>

	<layer name="daemon">
		<bean class="daemon.PsychicStopCommand"/>
	</layer>

	<console-modules>
		<console-module name="psychic"/>
	</console-modules>

</plugin>
