<console-module
	name="psychic-affiliate-group">

	<console-helper-provider
		object-name="psychicAffiliateGroup">

		<context-stuff
			name="psychicAffiliateGroupName"
			field="name"/>

		<context-stuff
			name="psychicId"
			delegate="psychic"
			field="id"/>

		<run-post-processor
			name="psychic"/>

	</console-helper-provider>

	<object-context
		name="psychicAffiliateGroup"
		object-name="psychicAffiliateGroup">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<context-link
				name="affiliate"
				link="psychicAffiliate"
				label="Affiliates"
				priv-key="psychic.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="psychicAffiliateGroup">

		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="psychicAffiliateGroup">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

	</field-set>

	<field-set
		name="settings"
		object-name="psychicAffiliateGroup">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

	</field-set>

</console-module>
