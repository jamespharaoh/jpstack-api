<console-module
	name="psychic-affiliate">

	<console-helper-provider
		object-name="psychicAffiliate">

		<context-stuff
			name="psychicAffiliateName"
			field="name"/>

		<context-stuff
			name="psychicAffiliateGroupId"
			delegate="psychicAffiliateGroup"
			field="id"/>

		<run-post-processor
			name="psychicAffiliateGroup"/>

	</console-helper-provider>

	<object-context
		name="psychicAffiliate"
		object-name="psychicAffiliate">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-sms-messages-page/>

			<object-sms-stats-page/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="psychicAffiliate">

		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="psychicAffiliate">

		<id-field/>
		<parent-field/>
			<code-field/>
			<name-field/>
			<description-field/>
			<deleted-field/>

		<text-field
			name="welcomeMessage"/>

	</field-set>

	<field-set
		name="settings"
		object-name="psychicAffiliate">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-area-field
			name="welcomeMessage"
			nullable="yes"
			char-count-function="gsmCharCountMultiple"
			char-count-data="undefined"/>

	</field-set>

</console-module>
