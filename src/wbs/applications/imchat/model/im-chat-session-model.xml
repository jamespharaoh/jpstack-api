<model-meta
	name="imChatSession"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="imChatCustomer"/>

		<code-field
			name="secret"/>

		<!-- timings -->

		<timestamp-field
			name="startTime"
			column-type="iso"/>

		<timestamp-field
			name="updateTime"
			column-type="iso"/>

		<timestamp-field
			name="endTime"
			column-type="iso"
			nullable="yes"/>

		<!-- state -->

		<yes-no-field
			name="active"/>

	</fields>

</model-meta>