<console-module
	name="im-chat-profile">

	<console-helper-provider
		object-name="imChatProfile"
		view-delegate-field="imChat"
		view-delegate-priv-code="manage">

		<priv-key
			name="imChatProfile.manage"
			delegate="imChat"
			priv="manage"/>

		<context-stuff
			name="imChatProfileName"
			field="code"/>

		<context-stuff
			name="imChatId"
			delegate="imChat"
			field="id"/>

		<run-post-processor
			name="imChat"/>

	</console-helper-provider>

	<object-context
		name="imChatProfile"
		object-name="imChatProfile">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="imChatProfile">

		<name-field/>
		<description-field/>

		<text-field
			name="publicName"/>

		<text-area-field
			name="publicDescription"/>

		<image-field
			name="profileImage"/>

	</field-set>

	<field-set
		name="create"
		object-name="imChatProfile">

		<name-field/>
		<description-field/>

		<enum-field
			name="state"
			helper="imChatProfileStateConsoleHelper"/>

		<text-field
			name="publicName"
			nullable="yes"/>

		<text-area-field
			name="publicDescription"
			nullable="yes"/>

		<image-field
			name="profileImage"
			nullable="yes"/>

	</field-set>

	<field-set
		name="summary"
		object-name="imChatProfile">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<enum-field
			name="state"
			helper="imChatProfileStateConsoleHelper"/>

		<text-field
			name="publicName"/>

		<text-area-field
			name="publicDescription"/>

		<image-field
			name="profileImage"/>

	</field-set>

	<field-set
		name="settings"
		object-name="imChatProfile">

		<name-field/>
		<description-field/>
		<deleted-field/>

		<enum-field
			name="state"
			helper="imChatProfileStateConsoleHelper"/>

		<text-field
			name="publicName"
			nullable="yes"/>

		<text-area-field
			name="publicDescription"
			nullable="yes"/>

		<image-field
			name="profileImage"
			nullable="yes"/>

	</field-set>

</console-module>
