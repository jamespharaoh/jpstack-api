<console-module
	name="im-chat-customer-detail-type">

	<console-helper-provider
		object-name="imChatCustomerDetailType"
		view-priv="imChat:manage">

		<priv-key
			name="imChatCustomerDetailType.manage"
			delegate="imChat"
			priv="manage"/>

		<context-stuff
			name="imChatCustomerDetailTypeName"
			field="name"/>

		<context-stuff
			name="imChatId"
			delegate="imChat"
			field="id"/>

		<run-post-processor
			name="imChat"/>

	</console-helper-provider>

	<object-context
		name="imChatCustomerDetailType"
		object-name="imChatCustomerDetailType">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="imChatCustomerDetailType">

		<name-field/>
		<description-field/>

		<yes-no-field
			name="required"/>

		<yes-no-field
			name="restricted"/>

		<enum-field
			name="dataType"
			helper="imChatCustomerDetailDataTypeConsoleHelper"/>

		<integer-field
			name="minimumAge"/>

	</field-set>

	<field-set
		name="create"
		object-name="imChatCustomerDetailType">

		<name-field/>
		<description-field/>

		<text-field
			name="label"/>

		<text-field
			name="help"/>

		<yes-no-field
			name="required"/>

		<yes-no-field
			name="restricted"/>

		<enum-field
			name="dataType"
			helper="imChatCustomerDetailDataTypeConsoleHelper"/>

		<integer-field
			name="minimumAge"
			nullable="yes"/>

		<integer-field
			name="ordering"/>

	</field-set>

	<field-set
		name="summary"
		object-name="imChatCustomerDetailType">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-field
			name="label"/>

		<text-field
			name="help"/>

		<yes-no-field
			name="required"/>

		<yes-no-field
			name="restricted"/>

		<enum-field
			name="dataType"
			helper="imChatCustomerDetailDataTypeConsoleHelper"/>

		<integer-field
			name="minimumAge"/>

		<integer-field
			name="ordering"/>

	</field-set>

	<field-set
		name="settings"
		object-name="imChatCustomerDetailType">

		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-field
			name="label"/>

		<text-field
			name="help"/>

		<yes-no-field
			name="required"/>

		<yes-no-field
			name="restricted"/>

		<enum-field
			name="dataType"
			helper="imChatCustomerDetailDataTypeConsoleHelper"/>

		<integer-field
			name="minimumAge"
			nullable="yes"/>

		<integer-field
			name="ordering"/>

	</field-set>

</console-module>
