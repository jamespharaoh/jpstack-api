<console-module
	name="im-chat-customer">

	<console-helper-provider
		object-name="imChatCustomer"
		view-delegate-field="imChat"
		view-delegate-priv-code="manage">

		<priv-key
			name="imChatCustomer.manage"
			delegate="imChat"
			priv="manage"/>

		<context-stuff
			name="imChatCustomerName"
			field="code"/>

		<context-stuff
			name="imChatId"
			delegate="imChat"
			field="id"/>

		<run-post-processor
			name="imChat"/>

	</console-helper-provider>

	<object-context
		name="imChatCustomer"
		object-name="imChatCustomer">

		<list>

			<object-search-page
				search-fields="search"
				results-fields="results"
				priv-key="imChat.manage"
				parent-id-key="imChatId"
				parent-id-name="imChatId"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<context-link
				name="conversation"
				link="imChatConversation"
				label="Conversations"
				priv-key="imChatCustomer.manage"/>

			<context-link
				name="session"
				link="imChatSession"
				label="Sessions"
				priv-key="imChatSession.manage"/>

			<context-link
				name="purchase"
				link="imChatPurchase"
				label="Purchases"
				priv-key="imChatPurchase.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="search"
		class="wbs.applications.imchat.model.ImChatCustomerSearch">

		<text-field
			name="code"
			nullable="yes"/>

	</field-set>

	<field-set
		name="results"
		object-name="imChatCustomer">

		<code-field/>

		<text-field
			name="email"/>

		<integer-field
			name="numConversations"
			label="Convs"/>

		<integer-field
			name="numPurchases"
			label="Purchs"/>

		<currency-field
			name="balance"
			currency="imChat.currency"/>

	</field-set>

	<field-set
		name="summary"
		object-name="imChatCustomer">

		<id-field/>
		<parent-field/>
		<code-field/>

		<text-field
			name="email"/>

		<integer-field
			name="numConversations"/>

		<integer-field
			name="numPurchases"/>

		<currency-field
			name="balance"
			currency="imChat.currency"/>

	</field-set>

</console-module>