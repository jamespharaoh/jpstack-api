<console-module
	name="im-chat-price-point">

	<console-helper-provider
		object-name="imChatPricePoint"
		view-delegate-field="imChat"
		view-delegate-priv-code="manage">

		<priv-key
			name="imChatPricePoint.manage"
			delegate="imChat"
			priv="manage"/>

		<context-stuff
			name="imChatPricePointName"
			field="code"/>

		<context-stuff
			name="imChatId"
			delegate="imChat"
			field="id"/>

		<run-post-processor
			name="imChat"/>

	</console-helper-provider>

	<object-context
		name="imChatPricePoint"
		object-name="imChatPricePoint">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="imChatPricePoint">

		<name-field/>
		<description-field/>

		<currency-field
			name="price"
			currency="imChat.currency"/>

		<currency-field
			name="value"
			currency="imChat.currency"/>

		<integer-field
			name="order"/>

	</field-set>

	<field-set
		name="create"
		object-name="imChatPricePoint">

		<name-field/>
		<description-field/>

		<text-field
			name="publicName"/>

		<text-field
			name="publicDescription"/>

		<currency-field
			name="price"
			currency="imChat.currency"/>

		<currency-field
			name="value"
			currency="imChat.currency"/>

		<integer-field
			name="order"/>

	</field-set>

	<field-set
		name="summary"
		object-name="imChatPricePoint">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-field
			name="publicName"/>

		<text-field
			name="publicDescription"/>

		<currency-field
			name="price"
			currency="imChat.currency"/>

		<currency-field
			name="value"
			currency="imChat.currency"/>

		<integer-field
			name="order"/>

	</field-set>

	<field-set
		name="settings"
		object-name="imChatPricePoint">

		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-field
			name="publicName"/>

		<text-field
			name="publicDescription"/>

		<currency-field
			name="price"
			currency="imChat.currency"/>

		<currency-field
			name="value"
			currency="imChat.currency"/>

		<integer-field
			name="order"/>

	</field-set>

</console-module>
