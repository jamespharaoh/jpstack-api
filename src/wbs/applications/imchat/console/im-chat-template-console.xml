<console-module
	name="im-chat-template">

	<console-helper-provider
		object-name="imChatTemplate"
		view-priv="imChat:manage">

		<context-stuff
			name="imChatTemplateName"
			field="name"/>

		<context-stuff
			name="imChatId"
			delegate="imChat"
			field="id"/>

		<priv-key
			name="imChatTemplate.manage"
			delegate="imChat"
			priv="manage"/>

		<run-post-processor
			name="imChat"/>

	</console-helper-provider>

	<object-context
		name="imChatTemplate"
		object-name="imChatTemplate">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="imChatTemplate">

		<name-field/>
		<description-field/>

		<text-field
			name="text"/>

	</field-set>

	<field-set
		name="create"
		object-name="imChatTemplate">

		<name-field/>
		<description-field/>

		<text-field
			name="text"/>

	</field-set>

	<field-set
		name="summary"
		object-name="imChatTemplate">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-area-field
			name="text"/>

	</field-set>

	<field-set
		name="settings"
		object-name="imChatTemplate">

		<id-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-area-field
			name="text"/>

	</field-set>

</console-module>