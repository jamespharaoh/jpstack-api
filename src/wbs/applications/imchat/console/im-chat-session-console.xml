<console-module
	name="im-chat-session">

	<console-helper-provider
		object-name="imChatSession"
		view-priv="grandparent:manage">

		<priv-key
			name="imChatSession.manage"
			delegate="imChatCustomer.imChat"
			priv="manage"/>

		<context-stuff
			name="imChatSessionName"
			field="secret"/>

		<context-stuff
			name="imChatCustomerId"
			delegate="imChatCustomer"
			field="id"/>

		<run-post-processor
			name="imChatCustomer"/>

	</console-helper-provider>

	<object-context
		name="imChatSession"
		object-name="imChatSession">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="imChatSession">

		<code-field/>

		<timestamp-field
			name="startTime"
			label="Start"/>

		<timestamp-field
			name="updateTime"
			label="Update"/>

		<timestamp-field
			name="endTime"
			label="End"/>

		<yes-no-field
			name="active"/>

	</field-set>

	<field-set
		name="summary"
		object-name="imChatSession">

		<id-field/>
		<parent-field/>
		<code-field/>

		<timestamp-field
			name="startTime"
			label="Start"/>

		<timestamp-field
			name="updateTime"
			label="Update"/>

		<timestamp-field
			name="endTime"
			label="End"/>

		<yes-no-field
			name="active"/>

	</field-set>

</console-module>
