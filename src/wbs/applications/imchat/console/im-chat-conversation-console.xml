<console-module
	name="im-chat-conversation">

	<console-helper-provider
		object-name="imChatConversation"
		view-delegate-field="grandparent"
		view-delegate-priv-code="manage">

		<priv-key
			name="imChatConversation.manage"
			delegate="imChatCustomer.imChat"
			priv="manage"/>

		<context-stuff
			name="imChatConversationName"
			field="index"/>

		<context-stuff
			name="imChatCustomerId"
			delegate="imChatCustomer"
			field="id"/>

		<run-post-processor
			name="imChatCustomer"/>

	</console-helper-provider>

	<object-context
		name="imChatConversation"
		object-name="imChatConversation">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<context-link
				name="message"
				link="imChatMessage"
				label="Messages"
				priv-key="imChatConversation.manage"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="imChatConversation">

		<index-field/>

		<object-field
			name="imChatProfile"
			label="Profile"
			object-type="imChatProfile"/>

		<integer-field
			name="numMessages"
			label="Messages"/>

		<timestamp-field
			name="startTime"/>

	</field-set>

	<field-set
		name="summary"
		object-name="imChatConversation">

		<id-field/>
		<parent-field/>
		<index-field/>

		<object-field
			name="imChatProfile"
			label="Profile"
			object-type="imChatProfile"/>

		<integer-field
			name="numMessages"/>

		<timestamp-field
			name="startTime"/>

		<integer-field
			name="freeMessages"/>

	</field-set>

</console-module>