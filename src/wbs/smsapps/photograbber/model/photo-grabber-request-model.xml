<model-meta
	name="photoGrabberRequest"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- common -->

		<parent-field
			type="photoGrabber"/>

		<code-field/>

		<!-- details -->

		<reference-field
			type="number"/>

		<integer-field
			name="threadId"/>

		<reference-field
			name="billedMessage"
			type="message"/>

		<yes-no-field
			name="found"/>

		<timestamp-field
			name="requestTime"
			column-type="postgresql"/>

		<timestamp-field
			name="responseTime"
			column-type="postgresql"/>

		<string-field
			name="mediaRef"/>

		<string-field
			name="mediaUrl"/>

		<reference-field
			type="media"/>

	</fields>

	<collections>

		<children-collection
			type="photoGrabberReissue"
			order-sql="timestamp DESC"/>

	</collections>

</model-meta>