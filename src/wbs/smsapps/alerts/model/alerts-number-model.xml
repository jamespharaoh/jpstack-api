<model-meta
	name="alertsNumber"
	type="ephemeral">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="alertsSettings"/>

		<identity-reference-field
			type="number"/>

		<!-- settings -->

		<string-field
			name="name"/>

		<yes-no-field
			name="enabled"/>

	</fields>

	<event-types>

		<event-type
			name="Alerts number created"
			text="%0 added number id %1 to %2"
			admin="yes"/>

		<event-type
			name="Alerts number deleted"
			text="%0 deleted number id %1 from %2"
			admin="yes"/>

		<event-type
			name="Alerts number updated"
			text="%0 set %1 for number id %2 of %3 to %4"
			admin="yes"/>

	</event-types>

</model-meta>