<model-meta
	name="alertsStatusCheck"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="alertsSettings"
			cacheable="yes"/>

		<index-field
			cacheable="yes"/>

		<!-- data -->

		<timestamp-field
			name="timestamp"
			column-type="sql"/>

		<integer-field
			name="unclaimedItems"/>

		<integer-field
			name="maximumDuration"/>

		<yes-no-field
			name="result"/>

		<yes-no-field
			name="active"/>

		<yes-no-field
			name="sentRecently"/>

		<yes-no-field
			name="alertSent"/>

	</fields>

</model-meta>