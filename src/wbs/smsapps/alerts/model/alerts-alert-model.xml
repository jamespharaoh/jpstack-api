<model-meta
	name="alertsAlert"
	type="event">

	<fields>

		<generated-id-field/>

		<parent-field
			type="alertsSettings"
			cacheable="yes"/>

		<index-field
			cacheable="yes"/>

		<timestamp-field
			name="timestamp"
			column-type="sql"/>

		<integer-field
			name="unclaimedItems"/>

		<integer-field
			name="maximumDuration"/>

		<reference-field
			name="text"
			type="text"/>

		<integer-field
			name="recipients"/>

	</fields>

</model-meta>