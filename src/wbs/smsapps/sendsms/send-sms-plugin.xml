<plugin
	name="send-sms"
	package="wbs.smsapps.sendsms">

	<plugin-dependencies>
		<plugin-dependency name="sms-apps-common"/>
	</plugin-dependencies>

	<models>
		<model name="sendSmsMessage"/>
		<model name="sendSmsConfig"/>
	</models>

	<fixtures>
		<fixture name="sendSms"/>
	</fixtures>

	<layer name="console">
		<component class="console.SendSmsConfigSendFormActionHelper"/>
	</layer>

	<console-modules>
		<console-module name="send-sms-config"/>
	</console-modules>

</plugin>
