<console-module
	name="broacast-number">

	<console-helper-provider
		object-name="broadcastNumber"
		view-priv="grandparent:manage">

		<context-stuff
			name="broadcastNumberName"
			field="number.number"/>

		<context-stuff
			name="broadcastId"
			field="broadcast.id"/>

		<run-post-processor
			name="broadcast"/>

	</console-helper-provider>

	<object-context
		name="broadcastNumber"
		object-name="broadcastNumber">

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="broadcastNumber">

		<parent-field/>

		<object-field
			name="number"
			object-type="number"/>

		<enum-field
			name="state"/>

		<object-field
			name="addedByUser"
			object-type="user"/>

		<object-field
			name="removedByUser"
			object-type="user"/>

		<object-field
			name="message"
			object-type="message"/>

	</field-set>

</console-module>
