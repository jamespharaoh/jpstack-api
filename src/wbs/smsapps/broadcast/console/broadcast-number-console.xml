<console-module
	name="broadcast-number">

	<console-helper-provider
		object-name="broadcastNumber"
		view-priv="grandparent:manage">

		<context-stuff
			name="broadcastNumberName"
			field="number.number"/>

		<context-stuff
			name="broadcastId"
			field="broadcast.id"/>

		<run-post-processor
			name="broadcast"/>

	</console-helper-provider>

	<object-context
		name="broadcastNumber"
		object-name="broadcastNumber">

		<object>

			<object-summary-page
				form="summary"/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="broadcast-number"
			type="read-only">

			<columns>

				<parent-field/>

				<object-field
					name="number"
					object-type="number"/>

				<enum-field
					name="state"/>

				<object-field
					name="addedByUser"
					object-type="user"/>

				<object-field
					name="removedByUser"
					object-type="user"/>

				<object-field
					name="message"
					object-type="message"/>

			</columns>

		</form>

	</forms>

</console-module>
