<model-meta
	name="forwarderMessageIn"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="forwarder"/>

		<!-- data -->

		<reference-field
			type="number"/>

		<reference-field
			type="message"/>

		<yes-no-field
			name="pending"/>

		<yes-no-field
			name="sendQueue"
			column="sendqueue"/>

		<timestamp-field
			name="createdTime"
			column-type="postgresql"
			nullable="yes"/>

		<timestamp-field
			name="retryTime"
			column-type="postgresql"
			column="retry_timestamp"
			nullable="yes"/>

		<timestamp-field
			name="processedTime"
			column-type="postgresql"
			nullable="yes"/>

		<timestamp-field
			name="borrowedTime"
			column-type="postgresql"
			nullable="yes"/>

		<timestamp-field
			name="cancelledTime"
			column-type="postgresql"
			nullable="yes"/>

		<!-- statistics -->

		<integer-field
			name="billRepliesSent"
			default="0"/>

		<integer-field
			name="freeRepliesSent"
			default="0"/>

	</fields>

</model-meta>