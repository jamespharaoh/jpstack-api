<model-meta
	name="forwarderMessageOut"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="forwarder"/>

		<!-- data -->

		<reference-field
			type="forwarderMessageIn"
			nullable="yes"/>

		<reference-field
			type="forwarderRoute"/>

		<reference-field
			type="number"/>

		<reference-field
			type="message"
			nullable="yes"/>

		<timestamp-field
			name="createdTime"
			column-type="postgresql"/>

		<string-field
			name="otherId"
			nullable="yes"/>

		<yes-no-field
			name="bill"/>

		<!-- state -->

		<integer-field
			name="reportIndexNext"
			default="0"/>

		<integer-field
			name="reportIndexPending"/>

		<timestamp-field
			name="reportRetryTime"
			column-type="postgresql"
			nullable="yes"/>

		<integer-field
			name="reportTries"/>

		<reference-field
			name="nextForwarderMessageOut"
			type="forwarderMessageOut"/>

		<reference-field
			name="prevForwarderMessageOut"
			type="forwarderMessageOut"/>

	</fields>

	<collections>

		<children-list
			name="reports"
			type="forwarderMessageOutReport"/>

	</collections>

</model-meta>