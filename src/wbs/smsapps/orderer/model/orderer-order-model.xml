<model-meta
	name="ordererOrder"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="orderer"/>

		<!-- data -->

		<reference-field
			type="number"/>

		<reference-field
			name="receivedMessage"
			type="message"/>

		<reference-field
			name="billedMessage"
			type="message"/>

		<timestamp-field
			name="receivedTime"
			column-type="postgresql"/>

		<timestamp-field
			name="deliveredTime"
			column-type="postgresql"
			nullable="yes"/>

		<string-field
			name="text"/>

	</fields>

</model-meta>