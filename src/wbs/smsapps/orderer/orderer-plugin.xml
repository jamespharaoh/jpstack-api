<plugin
	name="orderer"
	package="orderer">

	<dependencies>
		<project name="wbs-sms-apps">
			<plugin name="sms-apps-common"/>
		</project>
	</dependencies>

	<sql-scripts>
		<sql-data name="orderer-data"/>
	</sql-scripts>

	<models>
		<model name="orderer"/>
		<model name="ordererOrder"/>
	</models>

	<layer name="daemon">
		<bean class="daemon.OrdererCommandTypeHandler"/>
		<bean class="daemon.OrdererDeliveryNoticeHandler"/>
	</layer>

	<fixtures>
		<fixture name="orderer"/>
	</fixtures>

	<console-modules>
		<console-module name="orderer"/>
	</console-modules>

</plugin>
