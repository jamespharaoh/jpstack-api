<plugin
	name="orderer"
	package="wbs.smsapps.orderer">

	<plugin-dependencies>
		<plugin-dependency name="sms-apps-common"/>
	</plugin-dependencies>

	<models>
		<model name="orderer"/>
		<model name="ordererOrder"/>
	</models>

	<layer name="daemon">
		<bean class="daemon.OrdererCommandTypeHandler"/>
		<bean class="daemon.OrdererDeliveryNoticeHandler"/>
	</layer>

	<fixtures>
		<fixture name="orderer"/>
	</fixtures>

	<console-modules>
		<console-module name="orderer"/>
	</console-modules>

</plugin>
