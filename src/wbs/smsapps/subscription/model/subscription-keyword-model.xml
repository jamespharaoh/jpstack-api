<model-meta
	name="subscriptionKeyword"
	type="minor">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="subscription"/>

		<code-field
			name="keyword"/>

		<!-- details -->

		<description-field/>

		<deleted-field/>

		<!-- settings -->

		<reference-field
			type="subscriptionAffiliate"
			nullable="yes"/>

		<reference-field
			type="subscriptionList"
			nullable="yes"/>

	</fields>

	<command-types>

		<command-type
			name="Subscribe"
			description="Subscribe to a subscription service with a specific affiliate and/or list"/>

	</command-types>

</model-meta>