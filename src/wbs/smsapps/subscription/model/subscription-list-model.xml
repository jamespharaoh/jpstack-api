<model-meta
	name="subscriptionList"
	type="minor">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="subscription"/>

		<code-field/>

		<!-- details -->

		<name-field/>

		<description-field/>

		<deleted-field/>

		<!-- statistics -->

		<integer-field
			name="numSubscribers"
			default="0"/>

	</fields>

	<command-types>

		<command-type
			name="Subscribe"
			description="Subscribe to a subscription service with a specific list"/>

	</command-types>

	<service-types>

		<service-type
			name="Default"
			description="Subscription service list"/>

	</service-types>

</model-meta>