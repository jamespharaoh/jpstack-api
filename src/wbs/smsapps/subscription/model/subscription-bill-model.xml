<model-meta
	name="subscriptionBill"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="subscriptionNumber"
			cacheable="yes"/>

		<index-field
			cacheable="yes"/>

		<!-- data -->

		<timestamp-field
			name="createdTime"
			column-type="iso"/>

		<timestamp-field
			name="deliveredTime"
			column-type="iso"
			nullable="yes"/>

		<reference-field
			type="message"
			nullable="yes"/>

		<!-- state -->

		<enum-field
			name="state"
			type="subscriptionBillState"/>

	</fields>

</model-meta>