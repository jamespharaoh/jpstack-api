<model-meta
	name="subscriptionSub"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="subscriptionNumber"/>

		<index-field/>

		<!-- state -->

		<yes-no-field
			name="active"/>

		<!-- data -->

		<reference-field
			type="subscriptionList"
			nullable="yes"/>

		<reference-field
			type="subscriptionAffiliate"
			nullable="yes"/>

		<integer-field
			name="startedThreadId"
			nullable="yes"/>

		<integer-field
			name="endedThreadId"
			nullable="yes"/>

		<timestamp-field
			name="started"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="ended"
			column-type="iso"
			nullable="yes"/>

		<reference-field
			name="startedBy"
			type="user"
			nullable="yes"/>

		<reference-field
			name="endedBy"
			type="user"
			nullable="yes"/>

	</fields>

</model-meta>