<console-module
	name="subscription-send">

	<console-helper-provider
		object-name="subscriptionSend"
		view-priv="parent:manage">

		<priv-key
			name="subscriptionSend.manage"
			delegate="subscription"
			priv="manage"/>

		<context-stuff
			name="subscriptionSendName"
			field="id"/>

		<context-stuff
			name="subscriptionId"
			delegate="subscription"
			field="id"/>

		<run-post-processor
			name="subscription"/>

	</console-helper-provider>

	<object-context
		name="subscriptionSend"
		object-name="subscriptionSend">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"
				create-time="createdTime"
				create-user="createdUser"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<context-link
				name="part"
				link="subscriptionSendPart"
				label="Parts"
				priv-key="subscription.manage"/>

			<context-link
				name="number"
				link="subscriptionSendNumber"
				label="Numbers"
				priv-key="subscription.manage"/>

			<context-tab-action-page
				name="control"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="subscriptionSend">

		<index-field/>
		<description-field/>

		<object-field
			name="createdUser"
			object-type="user"/>

		<timestamp-field
			name="createdTime"/>

		<integer-field
			name="numRecipients"/>

		<enum-field
			name="state"/>

	</field-set>

	<field-set
		name="create"
		object-name="subscriptionSend">

		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="subscriptionSend">

		<id-field/>
		<index-field/>
		<description-field/>

		<object-field
			name="batch"
			object-type="batch"/>

		<object-field
			name="createdUser"
			label="Created by"
			object-type="user"/>

		<object-field
			name="sentUser"
			label="Sent by"
			object-type="user"/>

		<object-field
			name="cancelledUser"
			label="Cancelled by"
			object-type="user"/>

		<timestamp-field
			name="createdTime"/>

		<timestamp-field
			name="sentTime"/>

		<timestamp-field
			name="cancelledTime"/>

		<timestamp-field
			name="scheduledTime"/>

		<timestamp-field
			name="scheduledForTime"/>

		<integer-field
			name="numRecipients"/>

		<enum-field
			name="state"/>

	</field-set>

	<field-set
		name="settings"
		object-name="subscriptionSend">

		<description-field/>

	</field-set>

</console-module>
