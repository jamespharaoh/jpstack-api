<console-module
	name="subscription-send">

	<console-helper-provider
		object-name="subscriptionSend"
		view-delegate-field="parent"
		view-delegate-priv-code="manage">

		<priv-key
			name="subscriptionSend.manage"
			delegate="subscription"
			priv="manage"/>

		<context-stuff
			name="subscriptionSendName"
			field="id"/>

		<context-stuff
			name="subscriptionId"
			delegate="subscription"
			field="id"/>

		<run-post-processor
			name="subscription"/>

	</console-helper-provider>

	<object-context
		name="subscriptionSend"
		object-name="subscriptionSend">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"
				create-time="createdTime"
				create-user="createdUser"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="subscriptionSend">

		<index-field/>
		<description-field/>

	</field-set>

	<field-set
		name="create"
		object-name="subscriptionSend">

		<description-field/>

	</field-set>

</console-module>
