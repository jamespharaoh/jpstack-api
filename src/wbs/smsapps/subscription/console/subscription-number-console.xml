<console-module
	name="subscription-number">

	<console-helper-provider
		object-name="subscriptionNumber"
		view-priv="parent:manage">

		<priv-key
			name="subscriptionNumber.manage"
			priv="subscription:manage"/>

		<context-stuff
			name="subscriptionNumberName"
			field="id"/>

		<context-stuff
			name="subscriptionId"
			field="subscription.id"/>

		<run-post-processor
			name="subscription"/>

	</console-helper-provider>

	<object-context
		name="subscriptionNumber"
		object-name="subscriptionNumber">

		<list>

			<context-tab-page
				name="overview"/>

			<object-search-page
				search-class="wbs.smsapps.subscription.model.SubscriptionNumberSearch"
				search-form="search"
				results-form="results"
				priv-key="subscription.manage"
				parent-id-key="subscriptionId"
				parent-id-name="subscriptionId"/>

			<context-tab-action-page
				name="addRemove"
				tab-label="Add/remove"
				title="Subscription number add/remove"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="search"
			class="wbs.smsapps.subscription.model.SubscriptionNumberSearch"
			type="search">

			<columns>

				<text-field
					name="numberLike"
					label="Number"
					nullable="yes"/>

				<yes-no-field
					name="active"
					nullable="yes"/>

				<timestamp-field
					name="joinedAfter"
					nullable="yes"/>

				<timestamp-field
					name="joinedBefore"
					nullable="yes"/>

			</columns>

		</form>

		<form
			name="results"
			object-type="subscription-number"
			type="read-only">

			<columns>

				<object-field
					name="number"
					object-type="number"/>

				<yes-no-field
					name="active"/>

				<timestamp-field
					name="firstJoin"/>

				<object-field
					name="subscriptionList"
					label="List"
					object-type="subscriptionList"/>

				<object-field
					name="subscriptionAffiliate"
					label="Affiliate"
					object-type="subscriptionAffiliate"/>

			</columns>

		</form>

		<form
			name="add-remove"
			class="wbs.smsapps.subscription.console.SubscriptionNumberAddRemoveForm"
			type="perform">

			<columns>

				<object-field
					name="numberFormat"
					object-type="numberFormat"/>

				<object-field
					name="subscriptionAffiliate"
					label="Affiliate"
					object-type="subscriptionAffiliate"/>

				<object-field
					name="subscriptionList"
					label="List"
					object-type="subscriptionList"/>

				<text-area-field
					name="numbers"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="subscription-number"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="number"
					object-type="number"/>

				<yes-no-field
					name="active"/>

				<timestamp-field
					name="firstJoin"/>

				<timestamp-field
					name="lastJoin"/>

				<object-field
					name="subscriptionList"
					object-type="subscriptionList"/>

				<object-field
					name="subscriptionAffiliate"
					object-type="subscriptionAffiliate"/>

				<integer-field
					name="balance"/>

			</columns>

		</form>

	</forms>

</console-module>
