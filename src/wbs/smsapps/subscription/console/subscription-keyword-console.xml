<console-module
	name="subscription-keyword">

	<console-helper-provider
		object-name="subscriptionKeyword"
		view-delegate-field="subscription"
		view-delegate-priv-code="manage">

		<priv-key
			name="subscriptionKeyword.manage"
			delegate="subscription"
			priv="manage"/>

		<context-stuff
			name="subscriptionKeywordName"
			field="keyword"/>

		<context-stuff
			name="subscriptionId"
			delegate="subscription"
			field="id"/>

		<run-post-processor
			name="subscription"/>

	</console-helper-provider>

	<object-context
		name="subscriptionKeyword"
		object-name="subscriptionKeyword">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="subscriptionKeyword">

		<code-field/>
		<description-field/>

		<object-field
			name="subscriptionList"
			label="List"
			finder="subscriptionList"/>

		<object-field
			name="subscriptionAffiliate"
			label="Affiliate"
			finder="subscriptionAffiliate"/>

	</field-set>

	<field-set
		name="summary"
		object-name="subscriptionKeyword">

		<id-field/>
		<parent-field/>
		<code-field/>
		<description-field/>

		<object-field
			name="subscriptionList"
			label="List"
			finder="subscriptionList"/>

		<object-field
			name="subscriptionAffiliate"
			label="Affiliate"
			finder="subscriptionAffiliate"/>

	</field-set>

	<field-set
		name="settings"
		object-name="subscriptionKeyword">

		<id-field/>
		<parent-field read-only="yes"/>
		<code-field/>
		<description-field/>

		<object-field
			name="subscriptionList"
			label="List"
			finder="subscriptionList"/>

		<object-field
			name="subscriptionAffiliate"
			label="Affiliate"
			finder="subscriptionAffiliate"/>

	</field-set>

</console-module>
