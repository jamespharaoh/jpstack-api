<console-module
	name="subscription-keyword">

	<console-helper-provider
		object-name="subscriptionKeyword"
		view-priv="subscription:manage">

		<priv-key
			name="subscriptionKeyword.manage"
			priv="subscription:manage"/>

		<context-stuff
			name="subscriptionKeywordName"
			field="keyword"/>

		<context-stuff
			name="subscriptionId"
			field="subscription.id"/>

		<run-post-processor
			name="subscription"/>

	</console-helper-provider>

	<object-context
		name="subscriptionKeyword"
		object-name="subscriptionKeyword">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="subscription-keyword"
			type="read-only">

			<columns>

				<code-field/>
				<description-field/>

				<object-field
					name="subscriptionList"
					label="List"
					object-type="subscriptionList"/>

				<object-field
					name="subscriptionAffiliate"
					label="Affiliate"
					object-type="subscriptionAffiliate"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="subscription-keyword"
			type="create">

			<columns>

				<code-field/>
				<description-field/>

				<object-field
					name="subscriptionList"
					label="List"
					object-type="subscriptionList"/>

				<object-field
					name="subscriptionAffiliate"
					label="Affiliate"
					object-type="subscriptionAffiliate"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="subscription-keyword"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>
				<description-field/>
				<deleted-field/>

				<object-field
					name="subscriptionList"
					label="List"
					object-type="subscriptionList"/>

				<object-field
					name="subscriptionAffiliate"
					label="Affiliate"
					object-type="subscriptionAffiliate"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="subscription-keyword"
			type="update">

			<columns>

				<id-field/>
				<parent-field read-only="yes"/>
				<code-field/>
				<description-field/>
				<deleted-field/>

				<object-field
					name="subscriptionList"
					label="List"
					object-type="subscriptionList"/>

				<object-field
					name="subscriptionAffiliate"
					label="Affiliate"
					object-type="subscriptionAffiliate"/>

			</columns>

		</form>

	</forms>

</console-module>
