<console-module
	name="subscription-list">

	<console-helper-provider
		object-name="subscriptionList"
		view-delegate-field="subscription"
		view-delegate-priv-code="manage">

		<priv-key
			name="subscriptionList.manage"
			priv="manage"/>

		<priv-key
			name="subscriptionList.messages"
			priv="messages"/>

		<priv-key
			name="subscriptionList.stats"
			priv="stats"/>

		<context-stuff
			name="subscriptionListName"
			field="name"/>

		<context-stuff
			name="subscriptionId"
			delegate="subscription"
			field="id"/>

		<run-post-processor
			name="subscription"/>

	</console-helper-provider>

	<object-context
		name="subscriptionList"
		object-name="subscriptionList">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-sms-messages-page/>

			<object-sms-stats-page/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="subscriptionList">

		<name-field/>
		<description-field/>

		<integer-field
			name="numSubscribers"/>

	</field-set>

	<field-set
		name="create"
		object-name="subscriptionList">

		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="subscriptionList">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<integer-field
			name="numSubscribers"/>

	</field-set>

	<field-set
		name="settings"
		object-name="subscriptionList">

		<id-field/>
		<parent-field read-only="yes"/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

	</field-set>

</console-module>
