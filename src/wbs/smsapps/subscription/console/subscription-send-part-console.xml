<console-module
	name="subscription-send-part">

	<console-helper-provider
		object-name="subscriptionSendPart"
		view-priv="grandparent:manage"
		create-priv="parent:manage">

		<priv-key
			name="subscriptionSendPart.manage"
			priv="grandparent:manage"/>

		<context-stuff
			name="subscriptionSendPartName"
			field="subscriptionList.name"/>

		<context-stuff
			name="subscriptionSendId"
			field="subscriptionSend.id"/>

		<run-post-processor
			name="subscriptionSend"/>

	</console-helper-provider>

	<object-context
		name="subscriptionSendPart"
		object-name="subscriptionSendPart">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="subscription-send-part"
			type="read-only">

			<columns>

				<object-field
					name="subscriptionList"
					object-type="subscriptionList"/>

				<text-area-field
					name="text"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="subscription-send-part"
			type="read-only">

			<columns>

				<object-field
					name="subscriptionList"
					object-type="subscriptionList"/>

				<text-area-field
					name="text"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="subscription-send-part"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<deleted-field/>

				<object-field
					name="subscriptionList"
					object-type="subscriptionList"/>

				<text-area-field
					name="text"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="subscription-send-part"
			type="update">

			<columns>

				<deleted-field/>

				<text-area-field
					name="text"/>

			</columns>

		</form>

	</forms>

</console-module>
