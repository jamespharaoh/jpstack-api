<model-meta
	name="ticketerTicket"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="ticketer"/>

		<code-field
			name="ticket"/>

		<!-- data -->

		<reference-field
			type="number"/>

		<reference-field
			type="message"/>

		<timestamp-field
			name="createdTime"
			column-type="postgresql"/>

		<timestamp-field
			name="retrievedTime"
			column-type="postgresql"/>

		<timestamp-field
			name="expiresTime"
			column-type="postgresql"/>

	</fields>

</model-meta>