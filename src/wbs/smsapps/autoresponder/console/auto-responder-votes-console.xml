<console-module
	name="auto-responder-votes">

	<extend-context
		name="autoResponder.votes"
		base-name="autoResponder"
		extension-point="autoResponder.plugins"
		object-name="autoResponder">

		<context-tab-responder-page
			name="votes"/>

	</extend-context>

	<field-set
		name="auto-responder-votes"
		class="wbs.smsapps.autoresponder.console.AutoResponderVotesForm">

		<interval-field
			name="timePeriod"/>

	</field-set>

	<form-contexts>

		<form-context
			name="votes"
			class="wbs.smsapps.autoresponder.console.AutoResponderVotesForm"
			type="search"
			fields="auto-responder-votes"/>

	</form-contexts>

</console-module>
