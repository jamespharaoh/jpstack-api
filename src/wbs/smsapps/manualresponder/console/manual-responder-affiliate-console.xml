<console-module
	name="manual-responder-affiliate">

	<console-helper-provider
		object-name="manualResponderAffiliate"
		view-priv="manualResponder:manage">

		<priv-key
			name="manualResponderAffiliate.manage"
			priv="manualResponder:manage"/>

		<context-stuff
			name="manualResponderAffiliateName"
			field="smsCustomerAffiliate.name"/>

		<context-stuff
			name="manualResponderId"
			field="manualResponder.id"/>

		<run-post-processor
			name="manualResponder"/>

	</console-helper-provider>

	<object-context
		name="manualResponderAffiliate"
		object-name="manualResponderAffiliate">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="manualResponderAffiliate">

		<object-field
			name="smsCustomerAffiliate"
			object-type="smsCustomerAffiliate"
			root-field="manualResponder.smsCustomerManager"/>

	</field-set>

	<field-set
		name="create"
		object-name="manualResponderAffiliate">

		<object-field
			name="smsCustomerAffiliate"
			object-type="smsCustomerAffiliate"
			root-field="manualResponder.smsCustomerManager"/>

	</field-set>

	<field-set
		name="summary"
		object-name="manualResponderAffiliate">

		<id-field/>
		<parent-field/>

		<object-field
			name="smsCustomerAffiliate"
			object-type="smsCustomerAffiliate"
			root-field="manualResponder.smsCustomerManager"/>

		<deleted-field/>

	</field-set>

	<field-set
		name="settings"
		object-name="manualResponderAffiliate">

		<parent-field/>

		<object-field
			name="smsCustomerAffiliate"
			object-type="smsCustomerAffiliate"
			root-field="manualResponder.smsCustomerManager"/>

		<deleted-field/>

	</field-set>

</console-module>
