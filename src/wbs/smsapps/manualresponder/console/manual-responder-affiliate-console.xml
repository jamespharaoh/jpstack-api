<console-module
	name="manual-responder-affiliate">

	<console-helper-provider
		object-name="manualResponderAffiliate"
		view-priv="manualResponder:manage">

		<priv-key
			name="manualResponderAffiliate.manage"
			priv="manualResponder:manage"/>

		<context-stuff
			name="manualResponderAffiliateName"
			field="smsCustomerAffiliate.name"/>

		<context-stuff
			name="manualResponderId"
			field="manualResponder.id"/>

		<run-post-processor
			name="manualResponder"/>

	</console-helper-provider>

	<object-context
		name="manualResponderAffiliate"
		object-name="manualResponderAffiliate">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="manual-responder-affiliate"
			type="read-only">

			<columns>

				<object-field
					name="smsCustomerAffiliate"
					object-type="smsCustomerAffiliate"
					root-field="manualResponder.smsCustomerManager"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="manual-responder-affiliate"
			type="create">

			<columns>

				<object-field
					name="smsCustomerAffiliate"
					object-type="smsCustomerAffiliate"
					root-field="manualResponder.smsCustomerManager"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="manual-responder-affiliate"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="smsCustomerAffiliate"
					object-type="smsCustomerAffiliate"
					root-field="manualResponder.smsCustomerManager"/>

				<deleted-field/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="manual-responder-affiliate"
			type="update">

			<columns>

				<parent-field/>

				<object-field
					name="smsCustomerAffiliate"
					object-type="smsCustomerAffiliate"
					root-field="manualResponder.smsCustomerManager"/>

				<deleted-field/>

			</columns>

		</form>

	</forms>

</console-module>
