<console-module
	name="manual-responder-report">

	<console-helper-provider
		object-name="manualResponderReport"
		view-delegate-field="manualResponder"
		view-delegate-priv-code="manage"
		id-key="manualResponderRequestId">

		<context-stuff
			name="manualResponderRequestId"
			field="id"/>

		<run-post-processor
			name="manualResponderRequest"/>

	</console-helper-provider>

	<simple-context
		name="manualResponderReport">

		<context-tab-page
			name="simple"
			title="Simple"/>

		<context-responder-page
		    name="simpleCsv"/>

	</simple-context>

	<field-set
		name="simpleReportSearch"
		class="wbs.smsapps.manualresponder.console.ManualResponderReportSimplePart$SearchForm">

		<timestamp-from-field
			name="start"/>

		<timestamp-to-field
			name="end"/>

	</field-set>

	<field-set
		name="simpleReportResults"
		object-name="manualResponderReport">

		<object-field
			name="manualResponderRequest"
			label="Request"
			finder="manualResponderRequest"/>

		<text-field
		    name="timestring"
		    label="Timestamp"/>

		<object-field
			name="user"
			finder="user"/>

		<integer-field
			name="num"
			label="Messages"/>

	</field-set>

	<field-set
		name="simpleReportCsv"
		object-name="manualResponderReport">

		 <delegate-field
			delegate="manualResponder"
			name="code"
			label="Service"/>

		<text-field
			name="timestring"
			label="Timestamp"/>

		<delegate-field
			delegate="user"
			name="username"
			label="User"/>

		<integer-field
			name="num"
			label="Messages"/>

	</field-set>

</console-module>