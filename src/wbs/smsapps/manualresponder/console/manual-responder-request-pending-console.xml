<console-module
	name="manual-responder-request-pending">

	<object-context
		name="manualResponderRequest.pending"
		bean-name="manualResponderRequestPending"
		object-name="manualResponderRequest">

		<object>

			<context-tab-responder-page
				name="summary"/>

			<context-action-page
				name="form"/>

			<context-action
				name="numberNoteUpdate"/>

		</object>

	</object-context>

	<forms>

		<form
			name="customer"
			object-type="sms-customer"
			type="read-only">

			<column-fields>

				<code-field/>

				<date-field
					name="dateOfBirth"/>

			</column-fields>

		</form>

	</forms>

</console-module>
