<console-module
	name="manual-responder-shared-report">

	<console-helper-provider
		object-name="manualResponderReport"
		view-priv="manualResponder:manage"
		id-key="manualResponderRequestId">

		<context-stuff
			name="manualResponderRequestId"
			field="id"/>

		<run-post-processor
			name="manualResponderRequest"/>

	</console-helper-provider>

	<simple-context
		name="manualResponderSharedReport">

		<context-tab-page
			name="simple"
			title="Simple"/>

		<context-responder-page
			name="simpleCsv"/>

		<context-tab-page
			name="service"
			title="Service"/>

		<context-responder-page
			name="serviceCsv"/>

		<context-tab-page
			name="operator"
			title="Operator"/>

		<context-responder-page
			name="operatorCsv"/>

	</simple-context>

	<field-set
		name="simpleReportSearch"
		class="wbs.smsapps.manualresponder.console.ManualResponderSharedReportSimplePart$SearchForm">

		<timestamp-from-field
			name="start"/>

		<timestamp-to-field
			name="end"/>

	</field-set>

	<field-set
		name="simpleReportResults"
		object-name="manualResponderReport">

		<object-field
			name="manualResponderRequest"
			label="Request"
			object-type="manualResponderRequest"/>

		<text-field
			name="timestring"
			label="TimeStamp" />

		<object-field
			name="user"
			object-type="user"/>

		<integer-field
			name="num"
			label="Messages"/>

	</field-set>

	<!--
	<field-set
		name="serviceReportResults"
		object-name="wbs.smsapps.manualresponder.console.ManualResponderReportServicePart$ServiceCountForm">

		<text-field
			name="service"
			label="Service" />


		<integer-field
			name="num"
			label="Count"/>

	</field-set>
	-->

	<field-set
		name="simpleReportCsv"
		object-name="manualResponderReport">

		 <code-field
			delegate="manualResponder"
			label="Service"/>

		<text-field
			name="timestring"
			label="Timestamp"/>

		<code-field
			delegate="user"
			label="User"/>

		<integer-field
			name="num"
			label="Messages"/>

	</field-set>

</console-module>