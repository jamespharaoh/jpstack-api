<console-module
	name="manual-responder-shared-report">

	<simple-context
		name="manualResponderSharedReport">

		<object-search-page
			name="simple"
			tab-label="Simple"
			object-type="manualResponderRequest"
			search-class="wbs.smsapps.manualresponder.model.ManualResponderRequestSearch"
			search-fields="simple-report-search"
			results-fields="simple-report-results"/>

		<object-search-page
			name="service"
			tab-label="Service"
			object-type="manualResponderRequest"
			search-class="wbs.smsapps.manualresponder.model.ManualResponderRequestSearch"
			search-fields="service-report-search"
			search-dao-method="searchServiceReportIds"
			results-class="wbs.smsapps.manualresponder.model.ManualResponderServiceReport"
			results-fields="service-report-results"
			results-dao-method="searchServiceReports"/>

		<object-search-page
			name="operator"
			tab-label="Operator"
			object-type="manualResponderRequest"
			search-class="wbs.smsapps.manualresponder.model.ManualResponderRequestSearch"
			search-fields="operator-report-search"
			search-dao-method="searchOperatorReportIds"
			results-class="wbs.smsapps.manualresponder.model.ManualResponderOperatorReport"
			results-fields="operator-report-results"
			results-dao-method="searchOperatorReports"/>

	</simple-context>

	<!-- simple report fields -->

	<field-set
		name="simple-report-search"
		class="wbs.smsapps.manualresponder.model.ManualResponderRequestSearch">

		<interval-field
			name="processedTime"
			nullable="yes"/>

		<interval-field
			name="createdTime"
			nullable="yes"/>

	</field-set>

	<field-set
		name="simple-report-results"
		object-name="manualResponderRequest">

		<object-field
			name="request"
			object-type="manualResponderRequest"
			field="this"/>

		<timestamp-field
			name="createdTime"
			field="timestamp"/>

		<timestamp-field
			name="processedTime"/>

		<object-field
			name="user"/>

		<integer-field
			name="numBilledMessages"
			label="Billed"/>

		<integer-field
			name="numFreeMessages"
			label="Free"/>

	</field-set>

	<!-- service report fields -->

	<field-set
		name="service-report-search"
		class="wbs.smsapps.manualresponder.model.ManualResponderRequestSearch">

		<object-field
			name="operatorSlice"
			field="processedByUserSliceId"
			object-type="slice"
			nullable="yes"/>

		<interval-field
			name="processedTime"
			nullable="yes"/>

		<interval-field
			name="createdTime"
			nullable="yes"/>

	</field-set>

	<field-set
		name="service-report-results"
		class="wbs.smsapps.manualresponder.model.ManualResponderServiceReport">

		<object-field
			name="manualResponder"
			object-type="manualResponder"/>

		<integer-field
			name="numBilled"
			label="Billed"/>

		<integer-field
			name="numFree"
			label="Free"/>

		<integer-field
			name="numTotal"
			label="Total"/>

	</field-set>

	<!-- operator report -->

	<field-set
		name="operator-report-search"
		class="wbs.smsapps.manualresponder.model.ManualResponderRequestSearch">

		<object-field
			name="serviceSlice"
			field="manualResponderSliceId"
			object-type="slice"
			nullable="yes"/>

		<interval-field
			name="processedTime"
			nullable="yes"/>

		<interval-field
			name="createdTime"
			nullable="yes"/>

	</field-set>

	<field-set
		name="operator-report-results"
		class="wbs.smsapps.manualresponder.model.ManualResponderOperatorReport">

		<object-field
			name="user"
			object-type="user"/>

		<integer-field
			name="numBilled"
			label="Billed"/>

		<integer-field
			name="numFree"
			label="Free"/>

		<integer-field
			name="numTotal"
			label="Total"/>

	</field-set>

</console-module>