<model-meta
	name="manualResponderNumber"
	type="common">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="manualResponder"/>

		<identity-reference-field
			type="number"/>

		<!-- data -->

		<string-field
			name="code"
			nullable="no"/>

		<reference-field
			type="smsCustomer"
			nullable="yes"/>

		<!-- statistics -->

		<timestamp-field
			name="firstRequest"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="lastRequest"
			column-type="iso"
			nullable="yes"/>

		<integer-field
			name="numRequests"
			default="0"/>

		<!-- state -->

		<yes-no-field
			name="expectDateOfBirth"
			default="no"/>

		<yes-no-field
			name="bypassDateOfBirth"
			default="no"/>

		<!-- settings -->

		<reference-field
			name="notesText"
			type="text"
			nullable="yes"/>

	</fields>

</model-meta>