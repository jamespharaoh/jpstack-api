<console-module
	name="chat-graphs">

	<extend-context
		name="chatReport.graphs"
		base-name="chatReport"
		extension-point="chatReport.graphs"
		object-name="chat"
		component-name="chat"
		friendly-name="chat">

		<section
			name="graphs"
			label="Graphs">

			<context-tab-responder-page
				name="users"/>

			<context-responder-page
				name="usersImage"/>

			<context-tab-responder-page
				name="joiners"/>

			<context-responder-page
				name="joinersImage"/>

			<context-tab-responder-page
				name="dailyUsers"/>

			<context-responder-page
				name="dailyUsersImage"/>

		</section>

	</extend-context>

</console-module>

