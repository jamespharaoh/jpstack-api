<console-module
	name="chat-rebill-log">

	<console-helper-provider
		object-name="chatRebillLog"
		view-priv="chat:supervisor">

		<priv-key
			name="chatRebillLog.manage"
			delegate="root"
			priv="manage"/>

		<context-stuff
			name="chatRebillLogName"
			field="id"/>

		<context-stuff
			name="chatId"
			delegate="chat"
			field="id"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatRebillLog"
		object-name="chatRebillLog">

		<list>

			<object-search-page
				search-fields="search"
				results-fields="results"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="search"
		class="wbs.apn.chat.bill.model.ChatRebillLogSearch">

		<interval-field
			name="timestamp"
			nullable="yes"/>

		<object-field
			name="userId"
			object-type="user"
			nullable="yes"/>

		<interval-field
			name="lastAction"
			nullable="yes"/>

		<integer-field
			name="minimumCreditOwed"
			nullable="yes"/>

		<yes-no-field
			name="includeBlocked"
			nullable="yes"/>

		<yes-no-field
			name="includeFailed"
			nullable="yes"/>

		<integer-field
			name="numChatUsers"
			nullable="yes"/>

	</field-set>

	<field-set
		name="results"
		object-name="chatRebillLog">

		<timestamp-field
			name="timestamp"/>

		<object-field
			name="user"
			object-type="user"/>

		<timestamp-field
			name="lastAction"/>

		<yes-no-field
			name="includeBlocked"
			label="Blocked"/>

		<yes-no-field
			name="includeFailed"
			label="Failed"/>

		<integer-field
			name="numChatUsers"
			label="Chat users"/>

	</field-set>

	<field-set
		name="summary"
		object-name="chatRebillLog">

		<id-field/>
		<parent-field/>

		<timestamp-field
			name="timestamp"/>

		<object-field
			name="user"
			object-type="user"/>

		<timestamp-field
			name="lastAction"/>

		<integer-field
			name="minimumCreditOwed"/>

		<yes-no-field
			name="includeBlocked"/>

		<yes-no-field
			name="includeFailed"/>

		<integer-field
			name="numChatUsers"/>

	</field-set>

</console-module>
