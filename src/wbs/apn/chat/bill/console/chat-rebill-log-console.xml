<console-module
	name="chat-rebill-log">

	<console-helper-provider
		object-name="chatRebillLog"
		view-priv="chat:supervisor">

		<priv-key
			name="chatRebillLog.manage"
			priv="root:manage"/>

		<context-stuff
			name="chatRebillLogName"
			field="id"/>

		<context-stuff
			name="chatId"
			field="chat.id"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatRebillLog"
		object-name="chatRebillLog">

		<list>

			<object-search-page
				search-class="wbs.apn.chat.bill.model.ChatRebillLogSearch"
				search-form="search"
				results-form="results"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

		</object>

	</object-context>

	<forms>

		<form
			name="search"
			class="wbs.apn.chat.bill.model.ChatRebillLogSearch"
			type="search">

			<columns>

				<interval-field
					name="timestamp"
					nullable="yes"/>

				<object-field
					name="userId"
					object-type="user"
					nullable="yes"/>

				<interval-field
					name="lastAction"
					nullable="yes"/>

				<integer-field
					name="minimumCreditOwed"
					nullable="yes"/>

				<yes-no-field
					name="includeBlocked"
					nullable="yes"/>

				<yes-no-field
					name="includeFailed"
					nullable="yes"/>

				<integer-field
					name="numChatUsers"
					nullable="yes"/>

			</columns>

		</form>

		<form
			name="results"
			object-type="chat-rebill-log"
			type="read-only">

			<columns>

				<timestamp-field
					name="timestamp"/>

				<object-field
					name="user"
					object-type="user"/>

				<timestamp-field
					name="lastAction"/>

				<yes-no-field
					name="includeBlocked"
					label="Blocked"/>

				<yes-no-field
					name="includeFailed"
					label="Failed"/>

				<integer-field
					name="numChatUsers"
					label="Chat users"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="chat-rebill-log"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<timestamp-field
					name="timestamp"/>

				<object-field
					name="user"
					object-type="user"/>

				<timestamp-field
					name="lastAction"/>

				<integer-field
					name="minimumCreditOwed"/>

				<yes-no-field
					name="includeBlocked"/>

				<yes-no-field
					name="includeFailed"/>

				<integer-field
					name="numChatUsers"/>

			</columns>

		</form>

	</forms>

</console-module>
