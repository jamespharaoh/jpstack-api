<console-module
	name="chat-route-network">

	<console-helper-provider
		object-name="chatRouteNetwork"
		view-priv="chatRoute.chat:manage">

		<context-stuff
			name="chatRouteNetworkName"
			field="network.name"/>

		<context-stuff
			name="chatRouteId"
			field="chatRoute.id"/>

		<priv-key
			name="chatRouteNetwork.manage"
			priv="chatRoute.chat:manage"/>

		<run-post-processor
			name="chatRoute"/>

	</console-helper-provider>

	<object-context
		name="chatRouteNetwork"
		object-name="chatRouteNetwork">

		<list>

	`		<object-list-page
				form="list"/>

			<object-create-page
				form="create"
				create-priv-delegate="parent"
				create-priv-code="manage"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="chat-route-network"
			type="read-only">

			<columns>

				<object-field
					name="chatRoute"
					object-type="chatRoute"/>

				<object-field
					name="network"
					object-type="network"/>

				<integer-field
					name="outRev"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="chat-route-network"
			type="create">

			<columns>

				<parent-field/>

				<object-field
					name="network"
					object-type="network"/>

				<integer-field
					name="outRev"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="chat-route-network"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="network"
					object-type="network"/>

				<integer-field
					name="outRev"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="chat-route-network"
			type="update">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="network"
					object-type="network"/>

				<integer-field
					name="outRev"/>

			</columns>

		</form>

	</forms>

</console-module>