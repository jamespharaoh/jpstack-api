<console-module
	name="chat-route">

	<console-helper-provider
		object-name="chatRoute"
		view-priv="chat:manage">

		<context-stuff
			name="chatRouteName"
			field="route.name"/>

		<context-stuff
			name="chatId"
			field="chat.id"/>

		<priv-key
			name="chatRoute.manage"
			priv="chat:manage"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatRoute"
		object-name="chatRoute">

		<list>

	`		<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<context-link
				name="network"
				link="chatRouteNetwork"
				label="Networks"
				priv-key="chat.manage"/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="chat-route"
			type="read-only">

			<columns>

				<object-field
					name="route"
					object-type="route"/>

				<integer-field
					name="mmsCost"/>

				<integer-field
					name="smsCost"/>

				<integer-field
					name="inRev"/>

				<integer-field
					name="outRev"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="chat-route"
			type="create">

			<columns>

				<object-field
					name="route"
					object-type="route"/>

				<integer-field
					name="mmsCost"/>

				<integer-field
					name="smsCost"/>

				<integer-field
					name="inRev"/>

				<integer-field
					name="outRev"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="chat-route"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="route"
					object-type="route"/>

				<integer-field
					name="mmsCost"/>

				<integer-field
					name="smsCost"/>

				<integer-field
					name="inRev"/>

				<integer-field
					name="outRev"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="chat-route"
			type="update">

			<columns>

				<id-field/>

				<object-field
					name="route"
					object-type="route"/>

				<integer-field
					name="mmsCost"/>

				<integer-field
					name="smsCost"/>

				<integer-field
					name="inRev"/>

				<integer-field
					name="outRev"/>

			</columns>

		</form>

	</forms>

</console-module>