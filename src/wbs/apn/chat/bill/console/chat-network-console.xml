<console-module
	name="chat-network">

	<console-helper-provider
		object-name="chatNetwork"
		view-priv="chat:manage">

		<context-stuff
			name="chatNetworkName"
			field="network.name"/>

		<context-stuff
			name="chatId"
			field="chat.id"/>

		<priv-key
			name="chatNetwork.manage"
			priv="chat:manage"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatNetwork"
		object-name="chatNetwork">

		<list>

	`		<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="chatNetwork">

		<object-field
			name="network"
			object-type="network"/>

		<yes-no-field
			name="allowReverseBill"/>

		<yes-no-field
			name="allowPrePay"/>

	</field-set>

	<field-set
		name="create"
		object-name="chatNetwork">

		<object-field
			name="network"
			object-type="network"/>

		<yes-no-field
			name="allowReverseBill"/>

		<yes-no-field
			name="allowPrePay"/>

	</field-set>

	<field-set
		name="summary"
		object-name="chatNetwork">

		<id-field/>

		<parent-field/>

		<object-field
			name="network"
			object-type="network"/>

		<yes-no-field
			name="allowReverseBill"/>

		<yes-no-field
			name="allowPrePay"/>

	</field-set>

	<field-set
		name="settings"
		object-name="chatNetwork">

		<id-field/>

		<object-field
			name="network"
			object-type="network"/>

		<yes-no-field
			name="allowReverseBill"/>

		<yes-no-field
			name="allowPrePay"/>

	</field-set>

</console-module>