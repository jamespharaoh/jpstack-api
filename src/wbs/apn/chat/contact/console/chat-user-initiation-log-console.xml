<console-module
	name="chat-user-initiation-log">

	<console-helper-provider
		object-name="chatUserInitiationLog"
		view-priv="chatUser.chat:supervisor">

		<context-stuff
			name="chatUserInitiationLogName"
			field="id"/>

		<context-stuff
			name="chatUserId"
			field="chatUser.id"/>

		<priv-key
			name="chatUserInitiationLog.manage"
			priv="chatUser.chat:manage"/>

		<run-post-processor
			name="chatUser"/>

	</console-helper-provider>

	<object-context
		name="chatUserInitiationLog"
		object-name="chatUserInitiationLog">

		<object>

			<object-summary-page
				form="summary"/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="chat-user-initiation-log"
			type="read-only">

			<columns>

				<id-field/>

				<object-field
					name="chatUser"
					object-type="chatUser"/>

				<object-field
					name="monitorChatUser"
					object-type="chatUser"/>

				<object-field
					name="monitorUser"
					object-type="user"/>

				<timestamp-field
					name="timestamp"/>

				<timestamp-field
					name="alarmTime"/>

				<enum-field
					name="reason"/>

			</columns>

		</form>

	</forms>

</console-module>
