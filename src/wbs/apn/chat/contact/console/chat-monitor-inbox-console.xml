<console-module
	name="chat-monitor-inbox">

	<console-helper-provider
		object-name="chatMonitorInbox">

		<context-stuff
			name="chatMonitorInboxName"
			field="id"/>

		<context-stuff
			name="chatId"
			field="monitorChatUser.chat.id"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatMonitorInbox"
		object-name="chatMonitorInbox">

		<object>

			<context-tab-responder-page
				name="summary"/>

			<!-- TODO user -->
			<!-- TODO monitor -->

			<context-action-page
				name="form"/>

			<context-action
				name="alarm"/>

			<context-action
				name="addNote"/>

			<context-action
				name="updateNote"/>

			<context-action
				name="namedNoteUpdate"/>

		</object>

	</object-context>

</console-module>