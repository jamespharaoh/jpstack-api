<model-meta
	name="chatBlock"
	type="ephemeral">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chatUser"/>

		<identity-reference-field
			name="blockedChatUser"
			type="chatUser"/>

		<!-- timings -->

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

	</fields>

</model-meta>