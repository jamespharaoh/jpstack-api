<model-meta
	name="chatUserInitiationLog"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<reference-field
			name="chatUser"
			type="chatUser"/>

		<reference-field
			name="monitorChatUser"
			type="chatUser"/>

		<!-- TODO contact, index -->

		<!-- data -->

		<reference-field
			name="monitorUser"
			type="user"/>

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<timestamp-field
			name="alarmTime"
			column-type="postgresql"
			nullable="yes"/>

		<enum-field
			name="reason"
			type="chatUserInitiationReason"/>

	</fields>

</model-meta>