<model-meta
	name="chatMonitorInbox"
	type="ephemeral">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- data -->

		<reference-field
			name="monitorChatUser"
			type="chatUser"/>

		<reference-field
			name="userChatUser"
			type="chatUser"/>

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<reference-field
			type="queueItem"/>

		<yes-no-field
			name="inbound"
			default="no"/>

		<yes-no-field
			name="outbound"
			default="no"/>

	</fields>

</model-meta>