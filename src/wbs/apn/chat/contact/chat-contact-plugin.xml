<plugin
	name="chat-contact"
	package="wbs.apn.chat.contact">

	<sql-scripts>
		<sql-schema name="chat-message"/>
	</sql-scripts>

	<models>
		<custom-type name="chatMessageMethod"/>
		<custom-type name="chatMessageStatus"/>
		<custom-type name="chatUserInitiationReason"/>
		<model name="chatBlock"/>
		<model name="chatContact"/>
		<model name="chatContactNote"/>
		<model name="chatMessage"/>
		<model name="chatMessageView"/>
		<model name="chatMonitorInbox"/>
		<model name="chatUserInitiationLog"/>
	</models>

	<layer name="logic">
		<component class="logic.ChatMessageLogicImplementation"/>
		<component class="logic.ChatSendLogicImplementation"/>
	</layer>

	<layer name="console">
		<component class="console.ChatMessagePendingFormAction"/>
		<component class="console.ChatMessagePendingFormResponder"/>
		<component class="console.ChatMessageQueueConsolePlugin"/>
		<component class="console.ChatMonitorInboxAddNoteAction"/>
		<component class="console.ChatMonitorInboxAlarmAction"/>
		<component class="console.ChatMonitorInboxNamedNoteUpdateAction"/>
		<component class="console.ChatMonitorInboxFormAction"/>
		<component class="console.ChatMonitorInboxFormResponder"/>
		<component class="console.ChatMonitorInboxQueueConsolePlugin"/>
		<component class="console.ChatMonitorInboxSummaryPart"/>
		<component class="console.ChatMonitorInboxUpdateNoteAction"/>
		<component class="console.ChatUserInitiationLogConsoleHooks"/>
	</layer>

	<layer name="daemon">
		<component class="daemon.ChatChatCommand"/>
	</layer>

	<console-modules>
		<console-module name="chat-block"/>
		<console-module name="chat-contact"/>
		<console-module name="chat-contact-note"/>
		<console-module name="chat-message"/>
		<console-module name="chat-message-view"/>
		<console-module name="chat-monitor-inbox"/>
		<console-module name="chat-message-pending"/>
		<console-module name="chat-user-initiation-log"/>
	</console-modules>

</plugin>
