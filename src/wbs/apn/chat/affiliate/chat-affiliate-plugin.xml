<plugin
	name="chat-affiliate"
	package="wbs.apn.chat.affiliate">

	<sql-scripts>
		<sql-schema name="chat-affiliate-users-summary"/>
	</sql-scripts>

	<models>
		<model name="chatAffiliate"/>
		<model name="chatAffiliateUsersSummary"/>
	</models>

	<layer name="console">
		<component class="console.ChatAffiliateComparePart"/>
		<component class="console.ChatAffiliateCreateOldAction"/>
		<component class="console.ChatAffiliateCreateOldPart"/>
		<component class="console.ChatAffiliateKeywordsCreateAction"/>
		<component class="console.ChatAffiliateKeywordsCreatePart"/>
		<component class="console.ChatAffiliateKeywordsListPart"/>
		<component class="console.ChatAffiliateUsersPart"/>
	</layer>

	<console-modules>
		<console-module name="chat-affiliate"/>
	</console-modules>

</plugin>
