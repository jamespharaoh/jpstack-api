<plugin
	name="chat-affiliate"
	package="wbs.apn.chat.affiliate">

	<sql-scripts>
		<sql-schema name="chat-affiliate-users-summary"/>
		<sql-data name="chat-affiliate-data"/>
	</sql-scripts>

	<models>
		<model name="chatAffiliate"/>
		<model name="chatAffiliateUsersSummary"/>
	</models>

	<layer name="console">
		<bean class="console.ChatAffiliateComparePart"/>
		<bean class="console.ChatAffiliateCreateAction"/>
		<bean class="console.ChatAffiliateCreatePart"/>
		<bean class="console.ChatAffiliateKeywordsCreateAction"/>
		<bean class="console.ChatAffiliateKeywordsCreatePart"/>
		<bean class="console.ChatAffiliateKeywordsListPart"/>
		<bean class="console.ChatAffiliateUsersPart"/>
	</layer>

	<console-modules>
		<console-module name="chat-affiliate"/>
	</console-modules>

</plugin>
