<console-module
	name="chat-ad">

	<console-helper-provider
		object-name="chatAdTemplate"
		view-priv="chat:manage">

		<context-stuff
			name="chatAdTemplateName"
			field="id"/>

		<context-stuff
			name="chatId"
			field="chat:id"/>

		<priv-key
			name="chatAdTemplate.manage"
			priv="chat:manage"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatAdTemplate"
		object-name="chatAdTemplate">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-settings-page
				fields="settings"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="chatAdTemplate">

		<text-field
			name="genericText"/>

		<text-field
			name="gayMaleText"/>

		<text-field
			name="gayFemaleText"/>

	</field-set>

	<field-set
		name="create"
		object-name="chatAdTemplate">

		<parent-field/>

		<text-field
			name="genericText"/>

		<text-field
			name="gayMaleText"/>

		<text-field
			name="gayFemaleText"/>

	</field-set>

	<field-set
		name="settings"
		object-name="chatAdTemplate">

		<id-field/>

		<text-field
			name="genericText"/>

		<text-field
			name="gayMaleText"/>

		<text-field
			name="gayFemaleText"/>

	</field-set>

</console-module>
