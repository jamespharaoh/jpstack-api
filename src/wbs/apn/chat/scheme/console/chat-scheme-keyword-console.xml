<console-module
	name="chat-scheme-keyword">

	<console-helper-provider
		object-name="chatSchemeKeyword"
		view-priv="chatScheme">

		<context-stuff
			name="chatSchemeKeywordName"
			field="keyword"/>

		<context-stuff
			name="chatSchemeId"
			field="chatScheme.id"/>

		<priv-key
			name="chatSchemeKeyword.manage"
			priv="chatScheme:manage"/>

		<run-post-processor
			name="chatScheme"/>

	</console-helper-provider>

	<object-context
		name="chatSchemeKeyword"
		object-name="chatSchemeKeyword">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="chatSchemeKeyword">

		<code-field/>

		<enum-field
			name="joinType"
			label="Type"/>

		<enum-field
			name="joinGender"
			label="Gender"/>

		<enum-field
			name="joinOrient"
			label="Orient"/>

		<object-field
			name="joinChatAffiliate"
			label="Affiliate"
			object-type="chatAffiliate"/>

		<yes-no-field
			name="confirmCharges"
			label="Charges"/>

		<object-field
			name="command"
			object-type="command"/>

		<yes-no-field
			name="noCreditCheck"/>

	</field-set>

	<field-set
		name="summary"
		object-name="chatSchemeKeyword">

		<id-field/>
		<parent-field/>
		<code-field/>

		<enum-field
			name="joinType"/>

		<enum-field
			name="joinGender"/>

		<enum-field
			name="joinOrient"/>

		<object-field
			name="joinChatAffiliate"
			object-type="chatAffiliate"
			root-field="chatScheme"/>

		<yes-no-field
			name="confirmCharges"/>

		<object-field
			name="command"
			object-type="command"/>

		<yes-no-field
			name="noCreditCheck"/>

	</field-set>

	<field-set
		name="settings"
		object-name="chatSchemeKeyword">

		<id-field/>
		<code-field/>

		<enum-field
			name="joinType"
			nullable="yes"/>

		<enum-field
			name="joinGender"
			nullable="yes"/>

		<enum-field
			name="joinOrient"
			nullable="yes"/>

		<object-field
			name="joinChatAffiliate"
			object-type="chatAffiliate"
			root-field="chatScheme"
			nullable="yes"/>

		<yes-no-field
			name="confirmCharges"/>

		<object-field
			name="command"
			object-type="command"
			nullable="yes"/>

		<yes-no-field
			name="noCreditCheck"/>

	</field-set>

</console-module>
