<model-meta
	name="chatSchemeKeyword"
	type="minor">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chatScheme"/>

		<code-field
			name="keyword"/>

		<!-- settings -->

		<enum-field
			name="joinType"
			type="chatKeywordJoinType"
			nullable="yes"/>

		<enum-field
			name="joinGender"
			type="gender"
			nullable="yes"/>

		<enum-field
			name="joinOrient"
			type="orient"
			nullable="yes"/>

		<reference-field
			name="joinChatAffiliate"
			type="chatAffiliate"
			nullable="yes"/>

		<reference-field
			type="command"
			nullable="yes"/>

		<yes-no-field
			name="confirmCharges"
			default="no"/>

		<yes-no-field
			name="noCreditCheck"
			default="no"/>

	</fields>

</model-meta>