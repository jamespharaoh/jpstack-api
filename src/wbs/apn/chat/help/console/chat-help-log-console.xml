<console-module
	name="chat-help-log">

	<console-helper-provider
		object-name="chatHelpLog">

		<context-stuff
			name="chatHelpLogName"
			field="id"/>

		<context-stuff
			name="chatUserId"
			field="chatUser.id"/>

		<run-post-processor
			name="chatUser"/>

	</console-helper-provider>

	<object-context
		name="chatHelpLog.pending"
		bean-name="chatHelpLogPending"
		object-name="chatHelpLog">

		<object>

			<context-tab-responder-page
				name="summary"/>

			<context-tab-responder-page
				name="messages"/>

			<context-action-page
				name="form"/>

		</object>

	</object-context>

</console-module>
