<console-module
	name="chat-category">

	<console-helper-provider
		object-name="chatCategory"
		view-priv="chat">

		<context-stuff
			name="chatCategoryName"
			field="name"/>

		<context-stuff
			name="chatId"
			field="chat.id"/>

		<priv-key
			name="chatCategory.manage"
			priv="chat:manage"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatCategory"
		object-name="chatCategory">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="chatCategory">

		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="create"
		object-name="chatCategory">

		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="chatCategory">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

	</field-set>

	<field-set
		name="settings"
		object-name="chatCategory">

		<name-field/>
		<description-field/>
		<deleted-field/>

	</field-set>

</console-module>
