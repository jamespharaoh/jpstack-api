<plugin
	name="chat-info-site"
	package="wbs.apn.chat.infosite">

	<sql-scripts>
		<sql-schame name="chat-info-site"/>
	</sql-scripts>

	<models>
		<model name="chatInfoSite"/>
	</models>

	<layer name="api">
		<component class="api.ChatInfoSiteApiModule"/>
		<component class="api.ChatInfoSiteExpiredResponder"/>
		<component class="api.ChatInfoSiteImageResponder"/>
		<component class="api.ChatInfoSiteMessageSentResponder"/>
		<component class="api.ChatInfoSiteRespondAction"/>
		<component class="api.ChatInfoSiteViewAction"/>
		<component class="api.ChatInfoSiteViewResponder"/>
	</layer>

	<console-modules>
		<console-module name="chat-info-site"/>
	</console-modules>

</plugin>
