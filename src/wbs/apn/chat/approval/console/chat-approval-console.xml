<console-module
	name="chat-approval">

	<console-helper-provider
		object-name="chatApprovalRegexp"
		view-delegate-field="chat"
		view-delegate-priv-code="manage">

		<context-stuff
			name="chatApprovalRegexpName"
			field="id"/>

		<context-stuff
			name="chatId"
			delegate="chat"
			field="id"/>

		<priv-key
			name="chatApprovalRegexp.manage"
			delegate="chat"
			priv="manage"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatApprovalRegexp"
		object-name="chatApprovalRegexp">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page/>

		</list>

		<object>

			<object-settings-page
				fields="settings"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="chatApprovalRegexp">

		<text-field
			name="regexp"/>

		<yes-no-field
			name="auto"/>

	</field-set>

	<field-set
		name="settings"
		object-name="chatApprovalRegexp">

		<id-field/>

		<text-field
			name="regexp"/>

		<yes-no-field
			name="auto"/>

	</field-set>

</console-module>
