<model-meta
	name="chatUserSession"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chatUser"/>

		<timestamp-field
			name="startTime"
			column-type="postgresql"/>

		<timestamp-field
			name="endTime"
			column-type="postgresql"
			nullable="yes"/>

		<yes-no-field
			name="automatic"
			nullable="yes"/>

	</fields>

</model-meta>