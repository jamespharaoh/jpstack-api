<plugin
	name="chat-user"
	package="chat.user.core">

	<sql-scripts>
		<sql-schema name="chat-user"/>
		<sql-schema name="chat-user-session"/>
		<sql-schema name="chat-users-summary"/>
		<sql-data name="chat-user-data"/>
		<sql-data name="chat-users-summary-data"/>
	</sql-scripts>

	<models>
		<custom-type name="chatUserDateMode"/>
		<custom-type name="chatUserEditReason"/>
		<custom-type name="chatUserType"/>
		<custom-type name="gender"/>
		<custom-type name="orient"/>
		<model name="chatBadUser"/>
		<model name="chatUser"/>
		<model name="chatUserAlarm"/>
		<model name="chatUserDateLog"/>
		<model name="chatUserDobFailure"/>
		<model name="chatUserNote"/>
		<model name="chatUserSession"/>
		<model name="chatUsersSummary"/>
	</models>

	<layer name="logic">
		<bean class="logic.ChatUserLogicImpl"/>
	</layer>

	<layer name="console">
		<bean class="console.ChatUserConsoleConfig"/>
		<bean class="console.ChatUserCreateAction"/>
		<bean class="console.ChatUserCreatePart"/>
		<bean class="console.ChatUserHelpFormAction"/>
		<bean class="console.ChatUserHelpFormResponder"/>
		<bean class="console.ChatUserHelpPart"/>
		<bean class="console.ChatUserHistoryPart"/>
		<bean class="console.ChatUserNotesAction"/>
		<bean class="console.ChatUserNotesPart"/>
		<bean class="console.ChatUserNumberLinkProvider"/>
		<bean class="console.ChatUserOnlinePart"/>
		<bean class="console.ChatUserSearchAction"/>
		<bean class="console.ChatUserSearchPart"/>
		<bean class="console.ChatUserSearchResultsPart"/>
		<bean class="console.ChatUserSummaryPart"/>
	</layer>

	<layer name="daemon">
		<bean class="daemon.ChatMonitorSwapDaemon"/>
		<bean class="daemon.ChatUserAlarmDaemon"/>
		<bean class="daemon.ChatUserJoinOutboundDaemon"/>
		<bean class="daemon.ChatUserOnlineDaemon"/>
		<bean class="daemon.ChatUserQuietDaemon"/>
	</layer>

	<console-modules>
		<console-module name="chat-user"/>
	</console-modules>

</plugin>
