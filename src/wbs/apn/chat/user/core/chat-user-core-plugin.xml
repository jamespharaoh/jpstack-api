<plugin
	name="chat-user-core"
	package="wbs.apn.chat.user.core">

	<plugin-dependencies>
		<plugin-dependency name="media"/>
	</plugin-dependencies>

	<sql-scripts>
		<sql-schema name="chat-user"/>
		<sql-schema name="chat-user-session"/>
		<sql-schema name="chat-users-summary"/>
	</sql-scripts>

	<models>
		<custom-type name="chatUserDateMode"/>
		<custom-type name="chatUserEditReason"/>
		<custom-type name="chatUserType"/>
		<custom-type name="gender"/>
		<custom-type name="orient"/>
		<enum-type name="chatUserOperatorLabel"/>
		<model name="chatUser"/>
		<model name="chatUserAlarm"/>
		<model name="chatUserDateLog"/>
		<model name="chatUserDobFailure"/>
		<model name="chatUserNote"/>
		<model name="chatUserSession"/>
		<model name="chatUsersSummary"/>
	</models>

	<layer name="logic">
		<component class="logic.ChatUserLogicImplementation"/>
	</layer>

	<layer name="console">
		<component class="console.ChatUserConsoleHooks"/>
		<component class="console.ChatUserHelpFormAction"/>
		<component class="console.ChatUserHelpFormResponder"/>
		<component class="console.ChatUserHelpPart"/>
		<component class="console.ChatUserHistoryPart"/>
		<component class="console.ChatUserNotesAction"/>
		<component class="console.ChatUserNotesPart"/>
		<component class="console.ChatUserNumberLinkProvider"/>
		<component class="console.ChatUserOnlinePart"/>
		<component class="console.ChatUserSummaryPart"/>
	</layer>

	<layer name="daemon">
		<component class="daemon.ChatMonitorSwapDaemon"/>
		<component class="daemon.ChatUserAlarmDaemon"/>
		<component class="daemon.ChatUserJoinOutboundDaemon"/>
		<component class="daemon.ChatUserOnlineDaemon"/>
		<component class="daemon.ChatUserQuietDaemon"/>
	</layer>

	<console-modules>
		<console-module name="chat-user"/>
		<console-module name="chat-user-alarm"/>
		<console-module name="chat-user-date-log"/>
		<console-module name="chat-user-dob-failure"/>
		<console-module name="chat-user-note"/>
		<console-module name="chat-user-session"/>
		<console-module name="chat-users-summary"/>
	</console-modules>

</plugin>
