<plugin
	name="chat-user-info"
	package="wbs.apn.chat.user.info">

	<models>
		<custom-type name="chatUserInfoStatus"/>
		<model name="chatProfileField"/>
		<model name="chatProfileFieldValue"/>
		<model name="chatUserInfo"/>
		<model name="chatUserName"/>
		<model name="chatUserProfileField"/>
	</models>

	<layer name="logic">
		<component class="logic.ChatInfoLogicImplementation"/>
	</layer>

	<console-modules>
		<console-module name="chat-profile-field"/>
		<console-module name="chat-profile-field-value"/>
		<console-module name="chat-user-info"/>
		<console-module name="chat-user-name"/>
		<console-module name="chat-user-profile-field"/>
	</console-modules>

</plugin>
