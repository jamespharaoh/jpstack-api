<plugin
	name="chat-named-note"
	package="chat.namednote">

	<sql-scripts>
		<sql-schema name="chat-named-note"/>
		<sql-data name="chat-named-note-data"/>
	</sql-scripts>

	<models>
		<model name="chatNamedNote"/>
		<model name="chatNamedNoteLog"/>
		<model name="chatNoteName"/>
	</models>

	<layer name="console">
		<bean class="console.ChatNoteNamesAction"/>
		<bean class="console.ChatNoteNamesPart"/>
	</layer>

	<console-modules>
		<console-module name="chat-note-name"/>
	</console-modules>

</plugin>
