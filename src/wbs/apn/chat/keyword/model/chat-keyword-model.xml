<model-meta
	name="chatKeyword"
	type="ephemeral">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chat"/>

		<code-field
			name="keyword"/>

		<!-- settings -->

		<reference-field
			type="command"
			nullable="yes"/>

		<yes-no-field
			name="global"
			default="no"/>

		<yes-no-field
			name="chatBlock"
			default="no"/>

		<yes-no-field
			name="chatInfo"
			default="no"/>

		<enum-field
			name="joinType"
			type="chatKeywordJoinType"
			nullable="yes"/>

		<enum-field
			name="joinGender"
			type="gender"
			column="gender"
			nullable="yes"/>

		<enum-field
			name="joinOrient"
			type="orient"
			column="orient"
			nullable="yes"/>

		<reference-field
			name="joinChatAffiliate"
			type="chatAffiliate"
			nullable="yes"/>

		<yes-no-field
			name="noCreditCheck"
			default="no"/>

	</fields>

</model-meta>