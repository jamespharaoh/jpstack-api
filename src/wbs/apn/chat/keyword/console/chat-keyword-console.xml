<console-module
	name="chat-keyword">

	<console-helper-provider
		object-name="chatKeyword"
		view-priv="chat:manage">

		<context-stuff
			name="chatKeywordName"
			field="keyword"/>

		<context-stuff
			name="chatId"
			field="chat.id"/>

		<priv-key
			name="chatKeyword.manage"
			priv="chat:manage"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatKeyword"
		object-name="chatKeyword">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page/>

		</list>

		<object>

			<object-settings-page
				fields="settings"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="chatKeyword">

		<code-field/>

		<yes-no-field
			name="global"/>

		<yes-no-field
			name="chatBlock"/>

		<yes-no-field
			name="chatInfo"/>

		<object-field
			name="command"
			object-type="command"/>

		<yes-no-field
			name="noCreditCheck"/>

	</field-set>

	<field-set
		name="settings"
		object-name="chatKeyword">

		<id-field/>
		<code-field/>

		<yes-no-field
			name="global"/>

		<yes-no-field
			name="chatBlock"/>

		<yes-no-field
			name="chatInfo"/>

		<enum-field
			name="joinGender"
			nullable="yes"/>

		<enum-field
			name="joinOrient"
			nullable="yes"/>

		<enum-field
			name="joinType"
			nullable="yes"/>

		<object-field
			name="joinChatAffiliate"
			object-type="chatAffiliate"
			root-field="chat"/>

		<object-field
			name="command"
			object-type="command"/>

		<yes-no-field
			name="noCreditCheck"/>

	</field-set>

</console-module>
