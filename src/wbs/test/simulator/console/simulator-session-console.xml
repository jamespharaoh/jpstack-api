<console-module
	name="simulator-session">

	<console-helper-provider
		object-name="simulatorSession"
		view-priv="simulator:manage">

		<priv-key
			name="simulatorSession.manage"
			delegate="parent"
			priv="manage"/>

		<context-stuff
			name="simulatorSessionName"
			field="id"/>

		<context-stuff
			name="simulatorId"
			delegate="simulator"
			field="id"/>

		<run-post-processor
			name="simulator"/>

	</console-helper-provider>

	<object-context
		name="simulatorSession"
		object-name="simulatorSession"
		default-file="console">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"
				create-time="createdTime"
				create-user="createdUser"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<context-tab-responder-page
				name="console"/>

			<context-action
				name="createEvent"/>

			<context-get-action
				name="poll"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="simulatorSession">

		<description-field/>

		<timestamp-field
			name="createdTime"/>

		<object-field
			name="createdUser"
			object-type="user"/>

	</field-set>

	<field-set
		name="create"
		object-name="simulatorSession">

		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="simulatorSession">

		<id-field/>
		<parent-field/>
		<description-field/>

		<timestamp-field
			name="createdTime"/>

		<object-field
			name="createdUser"
			object-type="user"/>

	</field-set>

	<field-set
		name="settings"
		object-name="simulatorSession">

		<description-field/>

	</field-set>

</console-module>
