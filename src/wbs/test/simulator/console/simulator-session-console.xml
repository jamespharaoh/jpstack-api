<console-module
	name="simulator-session">

	<console-helper-provider
		object-name="simulatorSession"
		view-priv="simulator:manage">

		<priv-key
			name="simulatorSession.manage"
			priv="parent:manage"/>

		<context-stuff
			name="simulatorSessionName"
			field="id"/>

		<context-stuff
			name="simulatorId"
			field="simulator.id"/>

		<run-post-processor
			name="simulator"/>

	</console-helper-provider>

	<object-context
		name="simulatorSession"
		object-name="simulatorSession"
		default-file="console">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"
				create-time="createdTime"
				create-user="createdUser"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<context-tab-responder-page
				name="console"/>

			<context-action
				name="createEvent"/>

			<context-get-action
				name="poll"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="simulator-session"
			type="read-only">

			<columns>

				<description-field/>

				<timestamp-field
					name="createdTime"/>

				<object-field
					name="createdUser"
					object-type="user"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="simulator-session"
			type="create">

			<columns>

				<description-field/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="simulator-session"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<description-field/>

				<timestamp-field
					name="createdTime"/>

				<object-field
					name="createdUser"
					object-type="user"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="simulator-session"
			type="update">

			<columns>

				<description-field/>

			</columns>

		</form>

	</forms>

</console-module>
