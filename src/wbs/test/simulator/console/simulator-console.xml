<console-module
	name="simulator">

	<console-helper-provider
		object-name="simulator">

		<priv-key
			name="simulator.manage"
			priv="manage"/>

		<context-stuff
			name="simulatorName"
			field="name"/>

	</console-helper-provider>

	<object-context
		name="simulator"
		object-name="simulator">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<section
				name="settings"
				label="Settings">

				<object-settings-page
					short-name="general"
					fields="settings"/>

				<context-link
					name="route"
					link="simulatorRoute"
					label="Routes"
					priv-key="simulator.manage"/>

			</section>

			<context-link
				name="session"
				link="simulatorSession"
				label="Sessions"
				priv-key="simulator.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="simulator">

		<parent-field/>
		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="create"
		object-name="simulator">

		<parent-field/>
		<name-field/>
		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="simulator">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

	</field-set>

	<field-set
		name="settings"
		object-name="simulator">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

	</field-set>

</console-module>
