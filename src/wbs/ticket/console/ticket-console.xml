<console-module
	name="ticket">

	<console-helper-provider
		object-name="ticket"
		view-delegate-field="parent"
		view-delegate-priv-code="manage">
		
		<context-stuff
			name="ticketName"
			field="id"/>

		<context-stuff
			name="ticketId"
			field="id"/>

		<context-stuff
			name="ticketManagerId"
			delegate="ticketManager"
			field="id"/>

		<run-post-processor
			name="ticketManager"/>

	</console-helper-provider>

	<object-context
		name="ticket"
		object-name="ticket">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<context-link
				name="ticketNote"
				link="ticketNote"
				label="Ticket Notes"
				priv-key="ticket.ticketManager.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="ticket">

		<parent-field/>

	</field-set>

	<field-set
		name="create"
		object-name="ticket">

		<parent-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="ticket">

		<id-field/>
		<parent-field/>
		<code-field/>

	</field-set>

	<field-set
		name="settings"
		object-name="ticket">

		<parent-field/>

	</field-set>

</console-module>