<console-module
	name="chat-user-initiation-log">

	<console-helper-provider
		object-name="chatUserInitiationLog"
		view-delegate-field="chatUser.chat"
		view-delegate-priv-code="supervisor">

		<context-stuff
			name="chatUserInitiationLogName"
			field="id"/>

		<context-stuff
			name="chatUserId"
			delegate="chatUser"
			field="id"/>

		<priv-key
			name="chatUserInitiationLog.manage"
			delegate="chatUser.chat"
			priv="manage"/>

		<run-post-processor
			name="chatUser"/>

	</console-helper-provider>

	<object-context
		name="chatUserInitiationLog"
		object-name="chatUserInitiationLog">

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="chatUserInitiationLog">

		<id-field/>

		<object-field
			name="chatUser"
			object-type="chatUser"/>

		<object-field
			name="monitorChatUser"
			object-type="chatUser"/>

		<object-field
			name="monitorUser"
			object-type="user"/>

		<timestamp-field
			name="timestamp"/>

		<timestamp-field
			name="alarmTime"/>

		<enum-field
			name="reason"
			helper="chatUserInitiationReasonConsoleHelper"/>

	</field-set>

</console-module>
