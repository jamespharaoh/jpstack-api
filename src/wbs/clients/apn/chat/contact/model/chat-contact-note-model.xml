<model-meta
	name="chatContactNote"
	type="ephemeral">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- data -->

		<reference-field
			name="user"
			type="chatUser"/>

		<reference-field
			name="monitor"
			type="chatUser"/>

		<reference-field
			type="chat"/>

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<reference-field
			name="consoleUser"
			type="user"/>

		<!-- settings -->

		<string-field
			name="notes"
			default=""/>

		<yes-no-field
			name="pegged"
			default="no"/>

	</fields>

</model-meta>