<model-meta
	name="chatHelpLog"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chatUser"/>

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<!-- data -->

		<reference-field
			name="replyTo"
			type="chatHelpLog"
			nullable="yes"/>

		<reference-field
			type="message"/>

		<reference-field
			type="command"
			nullable="yes"/>

		<reference-field
			type="chatMessage"
			nullable="yes"/>

		<string-field
			name="text"/>

		<enum-field
			name="direction"
			type="messageDirection"/>

		<string-field
			name="ourNumber"/>

		<timestamp-field
			name="ignoredTime"
			column-type="postgresql"
			nullable="yes"/>

		<reference-field
			type="user"
			nullable="yes"/>

		<reference-field
			name="ignoredUser"
			type="user"
			nullable="yes"/>

		<reference-field
			type="queueItem"
			nullable="yes"/>

	</fields>

</model-meta>