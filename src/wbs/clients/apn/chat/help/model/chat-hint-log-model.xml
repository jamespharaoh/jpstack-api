<model-meta
	name="chatHintLog"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			name="user"
			type="chatUser"/>

		<identity-reference-field
			name="hint"
			type="chatHint"/>

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

	</fields>

</model-meta>