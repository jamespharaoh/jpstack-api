<model-meta
	name="chatHint"
	type="minor">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chat"/>

		<code-field/>

		<!-- details -->

		<name-field/>

		<description-field/>

		<deleted-field/>

		<!-- settings -->

		<yes-no-field
			name="sendIfOnline"/>

		<yes-no-field
			name="sendIfOffline"/>

		<string-field
			name="text"/>

	</fields>

</model-meta>