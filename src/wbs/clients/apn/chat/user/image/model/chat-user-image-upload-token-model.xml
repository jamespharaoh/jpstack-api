<model-meta
	name="chatUserImageUploadToken"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chatUser"
			cacheable="yes"/>

		<index-field
			cacheable="yes"/>

		<!-- secondary identity -->

		<string-field
			name="token"/>

		<!-- data -->

		<reference-field
			name="messageIn"
			type="message"
			nullable="yes"/>

		<reference-field
			type="user"
			nullable="yes"/>

		<reference-field
			name="messageOut"
			type="message"/>

		<timestamp-field
			name="createdTime"
			column-type="iso"/>

		<timestamp-field
			name="expiryTime"
			column-type="iso"/>

		<!-- statistics -->

		<timestamp-field
			name="firstViewTime"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="lastViewTime"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="firstExpiredTime"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="lastExpiredTime"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="firstUploadTime"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="lastUploadTime"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="firstFailedTime"
			column-type="iso"
			nullable="yes"/>

		<timestamp-field
			name="lastFailedTime"
			column-type="iso"
			nullable="yes"/>

		<integer-field
			name="numViews"
			default="0"/>

		<integer-field
			name="numExpired"
			default="0"/>

		<integer-field
			name="numUploads"
			default="0"/>

		<integer-field
			name="numFailures"
			default="0"/>

	</fields>

</model-meta>