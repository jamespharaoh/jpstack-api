<model-meta
	name="chatUserImage"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chatUser"/>

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<!-- data -->

		<reference-field
			type="media"/>

		<reference-field
			name="fullMedia"
			type="media"/>

		<reference-field
			type="message"
			nullable="yes"/>

		<enum-field
			name="status"
			type="chatUserInfoStatus"/>

		<reference-field
			name="moderator"
			type="user"
			column="user_id"
			nullable="yes"/>

		<timestamp-field
			name="moderationTime"
			column-type="postgresql"
			nullable="yes"/>

		<integer-field
			name="threadId"
			nullable="yes"/>

		<enum-field
			name="type"
			type="chatUserImageType"/>

		<integer-field
			name="index"
			nullable="yes"/>

		<yes-no-field
			name="append"
			nullable="yes"/>

		<string-field
			name="classification"
			default="unknown"/>

	</fields>

</model-meta>