<console-module
	name="chat-user">

	<console-helper-provider
		object-name="chatUser"
		view-delegate-field="chat"
		view-delegate-priv-code="supervisor">

		<context-stuff
			name="chatUserName"
			field="code"/>

		<context-stuff
			name="chatId"
			delegate="chat"
			field="id"/>

		<priv-key
			name="chatUser.view"
			delegate="chat"
			priv="supervisor"/>

		<priv-key
			name="chatUser.manage"
			delegate="chat"
			priv="manage"/>

		<run-post-processor
			name="chat"/>

	</console-helper-provider>

	<object-context
		name="chatUser"
		object-name="chatUser">

		<list>

			<context-tab-action-page
				name="search"/>

			<context-tab-responder
				name="searchResults"
				tab="chatUser.search"/>

			<context-tab-responder-page
				name="online"/>

			<context-tab-action-page
				name="create"/>

		</list>

		<object>

			<context-tab-responder-page
				name="summary"/>

			<context-tab-action-page
				name="notes"/>

			<extension-point
				name="chatUser.admin"/>

			<extension-point
				name="chatUser.images"/>

			<context-tab-responder-page
				name="history"/>

			<context-tab-responder-page
				name="help"/>

			<context-action-page
				name="helpForm"/>

			<object-events-page
				priv-key="chat.userAdmin"/>

		</object>

	</object-context>

</console-module>
