<model-meta
	name="chatUserDateLog"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chatUser"/>

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<!-- data -->

		<reference-field
			type="user"
			nullable="yes"/>

		<reference-field
			type="message"
			nullable="yes"/>

		<enum-field
			name="dateMode"
			type="chatUserDateMode"
			nullable="yes"/>

		<integer-field
			name="radius"/>

		<integer-field
			name="startHour"/>

		<integer-field
			name="endHour"/>

		<integer-field
			name="dailyMax"/>

	</fields>

</model-meta>