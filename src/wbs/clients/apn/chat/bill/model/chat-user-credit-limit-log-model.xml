<model-meta
	name="chatUserCreditLimitLog"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chatUser"/>

		<!-- data -->

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<integer-field
			name="oldCreditLimit"/>

		<integer-field
			name="newCreditLimit"/>

	</fields>

</model-meta>