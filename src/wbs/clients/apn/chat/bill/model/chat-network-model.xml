<model-meta
	name="chatNetwork"
	type="minor">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="chat"/>

		<identity-reference-field
			type="network"/>

		<!-- settings -->

		<yes-no-field
			name="allowReverseBill"
			default="no"/>

		<yes-no-field
			name="allowPrePay"
			default="no"/>

	</fields>

</model-meta>