<plugin
	name="chat-core"
	package="wbs.clients.apn.chat.core">

	<sql-scripts>
		<sql-schema name="chat-stats"/>
		<sql-data name="chat-data"/>
	</sql-scripts>

	<models>
		<model name="chat"/>
		<model name="chatStats"/>
		<model name="chatReport" />
		<model name="chatMonthCost"/>
	</models>

	<layer name="logic">
		<bean class="logic.ChatLogicHooksImpl"/>
		<bean class="logic.ChatMiscLogicImpl"/>
		<bean class="logic.ChatNumberLookupHelper"/>
		<bean class="logic.ChatNumberReportLogicImpl"/>
	</layer>

	<layer name="console">
		<bean class="console.ChatConsoleLogicImpl"/>
		<bean class="console.ChatKeywordJoinTypeConsoleHelper"/>
		<bean class="console.ChatStatisticsPart"/>
		<bean class="console.ChatUserCreditModeConsoleHelper"/>
		<bean class="console.ChatUserDateModeConsoleHelper"/>
		<bean class="console.GenderConsoleHelper"/>
		<bean class="console.OrientConsoleHelper"/>
	</layer>

	<layer name="daemon">
		<bean class="daemon.ChatDaemonConfig"/>
		<bean class="daemon.ChatBlockAllCommand"/>
		<bean class="daemon.ChatLogoffCommand"/>
		<bean class="daemon.ChatMagicCommand"/>
		<bean class="daemon.ChatMainCommand"/>
		<bean class="daemon.ChatNameCommand"/>
		<bean class="daemon.ChatPhotoCommand"/>
		<bean class="daemon.ChatSetPhotoCommand"/>
		<bean class="daemon.ChatVideoGetCommand"/>
		<bean class="daemon.ChatVideoSetCommand"/>
	</layer>

	<console-modules>
		<console-module name="chat"/>
		<console-module name="chat-month-cost"/>
	</console-modules>

</plugin>
