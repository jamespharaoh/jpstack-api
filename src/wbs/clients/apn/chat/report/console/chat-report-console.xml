<console-module
	name="chat-report">

	<console-helper-provider
		object-name="chatReport"
		view-delegate-field="chat"
		view-delegate-priv-code="manage"
		id-key="manualResponderRequestId">

	</console-helper-provider>

	<simple-context
		name="chatReport">

		<context-tab-page
			name="revShare"
			title="revShare"/>

	</simple-context>

	<field-set
		name="monthReportSearch"
		class="wbs.clients.apn.chat.report.console.ChatReportRevShareForm">

		<text-field
			name="month"/>

	</field-set>

	<field-set
		name="simpleReportResults"
		class="wbs.clients.apn.chat.report.console.ChatReportRevShareItem">

		<text-field
			name="path"
			label="Affiliate"/>

		<text-field
			name="description"
			label="Description"/>

		<integer-field
			name="joiners"
			label="Joiners"
			blank-if-zero="yes"/>

		<integer-field
			name="outRevNum"
			label="Out rev #"
			blank-if-zero="yes"/>

		<integer-field
			name="inRevNum"
			label="In rev #"
			blank-if-zero="yes"/>

		<integer-field
			name="smsCostNum"
			label="SMS cost #"
			blank-if-zero="yes"/>

		<integer-field
			name="mmsCostNum"
			label="MMS cost #"
			blank-if-zero="yes"/>

		<currency-field
			name="outRev_p"
			label="Out rev"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="inRev_p"
			label="In rev"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="smsCost_p"
			label="SMS cost"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="mmsCost_p"
			label="MMS cost"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="creditRev_p"
			label="Card rev"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="staffCost_p"
			label="Staff"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="total_p"
			label="Total"
			currency="currency"
			blank-if-zero="yes"/>

	</field-set>

</console-module>