<console-module
	name="chat-report">

	<simple-context
		name="chatReport">

		<context-tab-page
			name="revShare"
			title="Revenue share report"/>

		<object-search-page
			name="recentUsers"
			tab-label="Recent users"
			object-type="chatUser"
			search-class="wbs.platform.queue.model.ChatUserSearch"
			search-fields="recent-user-search"
			results-fields="recent-user-results"/>

	</simple-context>

	<field-set
		name="monthReportSearch"
		class="wbs.clients.apn.chat.report.console.ChatReportRevShareForm">

		<text-field
			name="month"/>

	</field-set>

	<field-set
		name="simpleReportResults"
		class="wbs.clients.apn.chat.report.console.ChatReportRevShareItem">

		<text-field
			name="path"
			label="Affiliate"/>

		<text-field
			name="description"
			label="Description"/>

		<integer-field
			name="joiners"
			label="Joiners"
			blank-if-zero="yes"/>

		<integer-field
			name="outRevNum"
			label="Out rev #"
			blank-if-zero="yes"/>

		<integer-field
			name="inRevNum"
			label="In rev #"
			blank-if-zero="yes"/>

		<integer-field
			name="smsCostNum"
			label="SMS cost #"
			blank-if-zero="yes"/>

		<integer-field
			name="mmsCostNum"
			label="MMS cost #"
			blank-if-zero="yes"/>

		<currency-field
			name="outRev_p"
			label="Out rev"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="inRev_p"
			label="In rev"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="smsCost_p"
			label="SMS cost"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="mmsCost_p"
			label="MMS cost"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="creditRev_p"
			label="Card rev"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="staffCost_p"
			label="Staff"
			currency="currency"
			blank-if-zero="yes"/>

		<currency-field
			name="total_p"
			label="Total"
			currency="currency"
			blank-if-zero="yes"/>

	</field-set>

	<field-set
		name="recent-user-search"
		class="wbs.clients.apn.chat.user.core.model.ChatUserSearch">

		<timestamp-partial-field
			name="lastJoin"
			nullable="yes"/>

	</field-set>

	<field-set
		name="recent-user-results"
		object-name="chatUser">

		<code-field/>

		<enum-field
			name="gender"
			helper="genderConsoleHelper"/>

		<enum-field
			name="orient"
			helper="orientConsoleHelper"/>

		<yes-no-field
			name="adultVerified"/>

		<image-field
			name="image"
			field="mainChatUserImage.media"/>

		<integer-field
			name="creditBought"/>

		<integer-field
			name="credit"/>

		<text-field
			name="locationPlace"/>

		<object-field
			name="chatAffiliate"
			object-type="chatAffiliate"/>

	</field-set>

</console-module>